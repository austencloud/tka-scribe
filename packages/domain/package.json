{
  "name": "@tka/domain",
  "version": "1.0.0",
  "description": "Shared domain models for TKA applications",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./models/*": {
      "import": "./dist/models/*.js",
      "types": "./dist/models/*.d.ts"
    },
    "./utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "json-schema-to-typescript": "^15.0.4",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "tka",
    "domain",
    "models",
    "types",
    "kinetic",
    "constructor"
  ],
  "author": "TKA Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/austencloud/TKA.git",
    "directory": "packages/domain"
  }
}
