{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ðŸš€ Debug TKA App (VS Code Optimized)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}",
      "preLaunchTask": "Start Dev Server",
      "userDataDir": "${workspaceFolder}/.vscode/chrome-debug-profile",
      "sourceMaps": true,
      "smartStep": true,
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**",
        "**/vite/**",
        "**/@vite/**"
      ],
      "trace": true,
      "pathMapping": {
        "/": "${workspaceFolder}/",
        "/@fs/": "/",
        "/src/": "${workspaceFolder}/src/"
      },
      "sourceMapPathOverrides": {
        "/@fs/*": "/*",
        "/src/*": "${workspaceFolder}/src/*",
        "/*": "${workspaceFolder}/*"
      },
      "disableNetworkCache": true,
      "showAsyncStacks": true
    },
    {
      "name": "ðŸ”— Debug TKA App (Attach to Running Server)",
      "type": "chrome",
