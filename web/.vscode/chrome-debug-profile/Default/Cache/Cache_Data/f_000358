/* ===== BASE SCROLLBAR VARIABLES ===== */

:root {
  /* Scrollbar dimensions */
  --glass-scrollbar-width: 12px;
  --glass-scrollbar-width-hover: 16px;
  --glass-scrollbar-minimal-width: 6px;
  --glass-scrollbar-track-border-radius: 6px;
  --glass-scrollbar-thumb-border-radius: 6px;

  /* Glassmorphism colors for scrollbars */
  --glass-scrollbar-track-bg: rgba(255, 255, 255, 0.03);
  --glass-scrollbar-track-border: rgba(255, 255, 255, 0.1);
  --glass-scrollbar-thumb-bg: rgba(255, 255, 255, 0.15);
  --glass-scrollbar-thumb-bg-hover: rgba(255, 255, 255, 0.25);
  --glass-scrollbar-thumb-bg-active: rgba(255, 255, 255, 0.35);
  --glass-scrollbar-thumb-border: rgba(255, 255, 255, 0.2);
  --glass-scrollbar-thumb-border-hover: rgba(255, 255, 255, 0.3);

  /* Shadow effects */
  --glass-scrollbar-track-shadow: inset 0 0 4px rgba(0, 0, 0, 0.1);
  --glass-scrollbar-thumb-shadow: 0 2px 8px rgba(0, 0, 0, 0.15);
  --glass-scrollbar-thumb-shadow-hover: 0 4px 12px rgba(0, 0, 0, 0.25);

  /* Transition timing */
  --glass-scrollbar-transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
  --glass-scrollbar-transition-fast: all 0.15s ease-out;
}

/* ===== PRIMARY GLASSMORPHISM SCROLLBAR ===== */

.glass-scrollbar-primary {
  /* Webkit browsers (Chrome, Safari, Edge) */
  &::-webkit-scrollbar {
    width: var(--glass-scrollbar-width);
    height: var(--glass-scrollbar-width);
  }

  &::-webkit-scrollbar-track {
    background: var(--glass-scrollbar-track-bg);
    border: 1px solid var(--glass-scrollbar-track-border);
    border-radius: var(--glass-scrollbar-track-border-radius);
    box-shadow: var(--glass-scrollbar-track-shadow);
    backdrop-filter: blur(10px);
    -webkit-backdrop-filter: blur(10px);
  }

  &::-webkit-scrollbar-thumb {
    background: var(--glass-scrollbar-thumb-bg);
    border: 1px solid var(--glass-scrollbar-thumb-border);
    border-radius: var(--glass-scrollbar-thumb-border-radius);
    box-shadow: var(--glass-scrollbar-thumb-shadow);
    backdrop-filter: blur(15px);
    -webkit-backdrop-filter: blur(15px);
    transition: var(--glass-scrollbar-transition);
  }

  &::-webkit-scrollbar-thumb:hover {
    background: var(--glass-scrollbar-thumb-bg-hover);
    border-color: var(--glass-scrollbar-thumb-border-hover);
    box-shadow: var(--glass-scrollbar-thumb-shadow-hover);
    transform: scale(1.05);
  }

  &::-webkit-scrollbar-thumb:active {
    background: var(--glass-scrollbar-thumb-bg-active);
    transform: scale(0.95);
    transition: var(--glass-scrollbar-transition-fast);
  }

  &::-webkit-scrollbar-corner {
    background: var(--glass-scrollbar-track-bg);
    border-radius: var(--glass-scrollbar-track-border-radius);
  }

  /* Firefox support */
  scrollbar-width: thin;
  scrollbar-color: var(--glass-scrollbar-thumb-bg)
    var(--glass-scrollbar-track-bg);
  scrollbar-gutter: stable;
}

/* ===== SECONDARY GLASSMORPHISM SCROLLBAR (More Subtle) ===== */

.glass-scrollbar-secondary {
  &::-webkit-scrollbar {
    width: calc(var(--glass-scrollbar-width) - 2px);
    height: calc(var(--glass-scrollbar-width) - 2px);
  }

  &::-webkit-scrollbar-track {
    background: rgba(255, 255, 255, 0.02);
    border: 1px solid rgba(255, 255, 255, 0.05);
    border-radius: var(--glass-scrollbar-track-border-radius);
    box-shadow: inset 0 0 2px rgba(0, 0, 0, 0.05);
    backdrop-filter: blur(8px);
    -webkit-backdrop-filter: blur(8px);
  }

  &::-webkit-scrollbar-thumb {
    background: rgba(255, 255, 255, 0.1);
    border: 1px solid rgba(255, 255, 255, 0.15);
    border-radius: var(--glass-scrollbar-thumb-border-radius);
    box-shadow: 0 1px 4px rgba(0, 0, 0, 0.1);
    backdrop-filter: blur(12px);
    -webkit-backdrop-filter: blur(12px);
    transition: var(--glass-scrollbar-transition);
  }

  &::-webkit-scrollbar-thumb:hover {
    background: rgba(255, 255, 255, 0.18);
    border-color: rgba(255, 255, 255, 0.25);
    box-shadow: 0 2px 6px rgba(0, 0, 0, 0.15);
  }

  &::-webkit-scrollbar-thumb:active {
    background: rgba(255, 255, 255, 0.25);
    transform: scale(0.98);
  }

  /* Firefox support */
  scrollbar-width: thin;
  scrollbar-color: rgba(255, 255, 255, 0.1) rgba(255, 255, 255, 0.02);
}

/* ===== MINIMAL GLASSMORPHISM SCROLLBAR (Ultra-thin) ===== */

