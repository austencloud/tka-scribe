import { createHotContext as __vite__createHotContext } from "/@vite/client";import.meta.hot = __vite__createHotContext("/src/lib/components/construct/option-picker/OptionPickerSection.svelte");import "/node_modules/.vite/deps/svelte_internal_disclose-version.js?v=cc8aba6c";

OptionPickerSection[$.FILENAME] = 'src/lib/components/construct/option-picker/OptionPickerSection.svelte';

import * as $ from "/node_modules/.vite/deps/svelte_internal_client.js?v=cc8aba6c";
import Pictograph from "/src/lib/components/pictograph/Pictograph.svelte";
import OptionPickerSectionHeader from "/src/lib/components/construct/option-picker/OptionPickerSectionHeader.svelte";

var on_click = (_, handlePictographSelected, pictograph) => handlePictographSelected($.get(pictograph));

var on_keydown = (e, handlePictographSelected, pictograph) => {
	if ($.strict_equals(e.key, "Enter") || $.strict_equals(e.key, " ")) {
		e.preventDefault();
		handlePictographSelected($.get(pictograph));
	}
};

var root_2 = $.add_locations($.from_html(`<div role="button" tabindex="0"><!></div>`), OptionPickerSection[$.FILENAME], [[75, 10]]);
var root_1 = $.add_locations($.from_html(`<div class="pictograph-frame s-XKmnBPKUj6VU"><div class="pictographs-grid s-XKmnBPKUj6VU"></div></div>`), OptionPickerSection[$.FILENAME], [[68, 4, [[69, 6]]]]);
var root_4 = $.add_locations($.from_html(`<div class="empty-section s-XKmnBPKUj6VU"><p class="s-XKmnBPKUj6VU">No options available for this type</p></div>`), OptionPickerSection[$.FILENAME], [[102, 4, [[103, 6]]]]);
var root = $.add_locations($.from_html(`<div role="region"><!> <!></div>`), OptionPickerSection[$.FILENAME], [[57, 0]]);

function OptionPickerSection($$anchor, $$props) {
	$.check_target(new.target);
	$.push($$props, true, OptionPickerSection);

	const pictographs = $.prop($$props, 'pictographs', 19, () => []),
		onPictographSelected = $.prop($$props, 'onPictographSelected', 3, () => {}),
		containerWidth = $.prop($$props, 'containerWidth', 3, 800),
		isExpanded = $.prop($$props, 'isExpanded', 3, true);

	let sectionExpanded = $.tag($.state($.proxy(isExpanded())), 'sectionExpanded');
	let selectedPictograph = $.tag($.state(null), 'selectedPictograph');

	const layoutConfig = $.tag(
		$.derived(() => () => {
			const baseSize = 144;
			const spacing = 8;
			const minSize = 100;
			const maxSize = 200;
			const availableWidth = containerWidth() - 32;
			const optionsPerRow = Math.max(1, Math.floor(availableWidth / (baseSize + spacing)));
			const totalSpacing = (optionsPerRow - 1) * spacing;
			const availableForPictographs = availableWidth - totalSpacing;
			let pictographSize = availableForPictographs / optionsPerRow;

			pictographSize = Math.min(maxSize, Math.max(minSize, pictographSize));

			if (pictographSize < 144 && pictographSize > 120) {
				pictographSize = 144;
			}

			return {
				optionsPerRow,
				pictographSize: Math.floor(pictographSize),
				spacing
			};
		}),
		'layoutConfig'
	);

	function toggleSection() {
		$.set(sectionExpanded, !$.get(sectionExpanded));
	}

	function handlePictographSelected(pictograph) {
		$.set(selectedPictograph, pictograph, true);
		onPictographSelected()(pictograph);
	}

	const sectionPictographs = $.tag(
		$.derived(() => () => {
			return pictographs();
		}),
		'sectionPictographs'
	);

	var div = root();
	let classes;
	var node = $.child(div);

	$.add_svelte_meta(
		() => OptionPickerSectionHeader(node, {
			get letterType() {
				return $$props.letterType;
			},

			onToggle: toggleSection
		}),
		'component',
		OptionPickerSection,
		64,
		2,
		{ componentTag: 'OptionPickerSectionHeader' }
	);

	var node_1 = $.sibling(node, 2);

	{
		var consequent = ($$anchor) => {
			var div_1 = root_1();
			var div_2 = $.child(div_1);
			let styles;

			$.validate_each_keys(() => $.get(sectionPictographs)(), (pictograph) => pictograph.id);

			$.add_svelte_meta(
				() => $.each(div_2, 21, () => $.get(sectionPictographs)(), (pictograph) => pictograph.id, ($$anchor, pictograph) => {
					var div_3 = root_2();
					let classes_1;

					div_3.__click = [on_click, handlePictographSelected, pictograph];
					div_3.__keydown = [on_keydown, handlePictographSelected, pictograph];

					let styles_1;
					var node_2 = $.child(div_3);

					$.add_svelte_meta(
						() => Pictograph(node_2, {
							get pictographData() {
								return $.get(pictograph);
							},

							get width() {
								return $.get(layoutConfig)().pictographSize;
							},

							get height() {
								return $.get(layoutConfig)().pictographSize;
							},

							debug: false
						}),
						'component',
						OptionPickerSection,
						91,
						12,
						{ componentTag: 'Pictograph' }
					);

					$.reset(div_3);

					$.template_effect(
						($0, $1) => {
							classes_1 = $.set_class(div_3, 1, 'pictograph-container s-XKmnBPKUj6VU', null, classes_1, $0);
							$.set_attribute(div_3, 'aria-label', `Select ${($.get(pictograph).letter || 'pictograph') ?? ''} pictograph`);
							styles_1 = $.set_style(div_3, '', styles_1, $1);
						},
						[
							() => ({
								selected: $.strict_equals($.get(selectedPictograph)?.id, $.get(pictograph).id)
							}),

							() => ({
								width: `${$.get(layoutConfig)().pictographSize ?? ''}px`,
								height: `${$.get(layoutConfig)().pictographSize ?? ''}px`
							})
						]
					);

					$.append($$anchor, div_3);
				}),
				'each',
				OptionPickerSection,
				74,
				8
			);

			$.reset(div_2);
			$.reset(div_1);

			$.template_effect(($0) => styles = $.set_style(div_2, '', styles, $0), [
				() => ({
					'grid-template-columns': `repeat(${$.get(layoutConfig)().optionsPerRow ?? ''}, 1fr)`,
					gap: `${$.get(layoutConfig)().spacing ?? ''}px`
				})
			]);

			$.append($$anchor, div_1);
		};

		var alternate = ($$anchor) => {
			var fragment = $.comment();
			var node_3 = $.first_child(fragment);

			{
				var consequent_1 = ($$anchor) => {
					var div_4 = root_4();

					$.append($$anchor, div_4);
				};

				$.add_svelte_meta(
					() => $.if(
						node_3,
						($$render) => {
							if ($.get(sectionExpanded) && $.strict_equals($.get(sectionPictographs)().length, 0)) $$render(consequent_1);
						},
						true
					),
					'if',
					OptionPickerSection,
					101,
					2
				);
			}

			$.append($$anchor, fragment);
		};

		$.add_svelte_meta(
			() => $.if(node_1, ($$render) => {
				if ($.get(sectionExpanded) && $.get(sectionPictographs)().length > 0) $$render(consequent); else $$render(alternate, false);
			}),
			'if',
			OptionPickerSection,
			67,
			2
		);
	}

	$.reset(div);

	$.template_effect(
		($0) => {
			classes = $.set_class(div, 1, 'option-picker-section s-XKmnBPKUj6VU', null, classes, $0);
			$.set_attribute(div, 'aria-label', `${$$props.letterType ?? ''} pictographs section`);
		},
		[() => ({ expanded: $.get(sectionExpanded) })]
	);

	$.append($$anchor, div);

	return $.pop({ ...$.legacy_api() });
}

if (import.meta.hot) {
	OptionPickerSection = $.hmr(OptionPickerSection, () => OptionPickerSection[$.HMR].source);

	import.meta.hot.acceptExports(["default"],(module) => {
		$.cleanup_styles('s-XKmnBPKUj6VU');
		module.default[$.HMR].source = OptionPickerSection[$.HMR].source;
		$.set(OptionPickerSection[$.HMR].source, module.default[$.HMR].original);
	});
}

export default OptionPickerSection;

$.delegate(['click', 'keydown']);
import "/src/lib/components/construct/option-picker/OptionPickerSection.svelte?svelte&type=style&lang.css";

