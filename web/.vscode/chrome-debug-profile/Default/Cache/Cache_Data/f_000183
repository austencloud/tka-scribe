import { createHotContext as __vite__createHotContext } from "/@vite/client";import.meta.hot = __vite__createHotContext("/src/routes/motion-tester/components/StaticSection.svelte");import "/node_modules/.vite/deps/svelte_internal_disclose-version.js?v=de368f3a";

StaticSection[$.FILENAME] = 'src/routes/motion-tester/components/StaticSection.svelte';

import * as $ from "/node_modules/.vite/deps/svelte_internal_client.js?v=de368f3a";
import Pictograph from "/src/lib/components/pictograph/Pictograph.svelte";
import PropPanel from "/src/routes/motion-tester/components/PropPanel.svelte";
import SimpleGridToggle from "/src/routes/motion-tester/components/SimpleGridToggle.svelte";
import { resolve } from "/src/lib/services/bootstrap.ts";
import { ICsvDataServiceInterface } from "/src/lib/services/di/interfaces/codex-interfaces.ts";
import { IArrowPositioningOrchestratorInterface } from "/src/lib/services/di/interfaces/positioning-interfaces.ts";
import { MotionType, Orientation, RotationDirection, GridMode } from "/src/lib/domain/enums.ts";

var root_2 = $.add_locations($.from_html(`<div class="error-state s-aeptAB2-JEBZ"><span class="error-icon s-aeptAB2-JEBZ">⚠️</span> <p class="s-aeptAB2-JEBZ">Unable to display pictograph</p></div>`), StaticSection[$.FILENAME], [[154, 8, [[155, 10], [156, 10]]]]);

var root = $.add_locations($.from_html(`<div class="static-section s-aeptAB2-JEBZ"><div class="pictograph-area s-aeptAB2-JEBZ"><div class="grid-toggle-overlay s-aeptAB2-JEBZ"><!></div> <div class="pictograph-container s-aeptAB2-JEBZ"><!></div></div> <div class="motion-controls s-aeptAB2-JEBZ"><div class="prop-section s-aeptAB2-JEBZ"><!></div> <div class="prop-section s-aeptAB2-JEBZ"><!></div></div></div>`), StaticSection[$.FILENAME], [
	[
		138,
		0,

		[
			[140, 2, [[141, 4], [145, 4]]],
			[163, 2, [[165, 4], [193, 4]]]
		]
	]
]);

function StaticSection($$anchor, $$props) {
	$.check_target(new.target);
	$.push($$props, true, StaticSection);

	const PICTOGRAPH_SIZE = 320;
	const csvDataService = resolve(ICsvDataServiceInterface);
	const arrowPositioningService = resolve(IArrowPositioningOrchestratorInterface);
	let pictographData = $.tag($.state(null), 'pictographData');

	$.user_effect(() => {
		const blueParams = $$props.motionState.blueMotionParams;
		const redParams = $$props.motionState.redMotionParams;
		const gridType = $$props.motionState.gridType;

		updatePictographData();
	});

	async function findLetterForMotionParams() {
		try {
			(await $.track_reactivity_loss(csvDataService.loadCsvData()))();

			if (!csvDataService.isReady()) {
				console.error("\u274C CSV data service not ready");

				return null;
			}

			const gridMode = $.strict_equals($$props.motionState.gridType, "diamond") ? GridMode.DIAMOND : GridMode.BOX;
			const csvRows = csvDataService.getParsedData(gridMode);

			if (!csvRows || $.strict_equals(csvRows.length, 0)) {
				console.error(`\u274C No CSV data available for grid mode: ${gridMode}`);

				return null;
			}

			console.log(`\u{1F50D} Searching ${csvRows.length} CSV rows for matching motion parameters...`);

			const matchingRow = csvRows.find((row) => {
				const blueMatch = matchesMotionParams(row, "blue", $$props.motionState.blueMotionParams);
				const redMatch = matchesMotionParams(row, "red", $$props.motionState.redMotionParams);

				return blueMatch && redMatch;
			});

			if (matchingRow) {
				console.log(...$.log_if_contains_state('log', `\u2705 Found exact match for letter "${matchingRow.letter}":`, matchingRow));

				return matchingRow.letter;
			} else {
				console.warn("\u26A0\uFE0F No matching letter found for current motion parameters");

				return null;
			}
		} catch(error) {
			console.error(...$.log_if_contains_state('error', "\u274C Error finding letter for motion parameters:", error));

			return null;
		}
	}

	function matchesMotionParams(row, color, params) {
		try {
			const startLocationCol = `${color}StartLocation`;
			const endLocationCol = `${color}EndLocation`;
			const motionTypeCol = `${color}MotionType`;
			const rowStartLocation = row[startLocationCol];
			const rowEndLocation = row[endLocationCol];
			const rowMotionType = row[motionTypeCol];

			if (!rowStartLocation || !rowEndLocation || !rowMotionType) {
				return false;
			}

			const startMatch = $.strict_equals(normalizeLocation(rowStartLocation), normalizeLocation(params.startLocation));
			const endMatch = $.strict_equals(normalizeLocation(rowEndLocation), normalizeLocation(params.endLocation));
			const motionMatch = $.strict_equals(normalizeMotionType(rowMotionType), normalizeMotionType(params.motionType));

			return startMatch && endMatch && motionMatch;
		} catch(error) {
			return false;
		}
	}

	function normalizeLocation(location) {
		return location?.toLowerCase().trim() || "";
	}

	function normalizeMotionType(motionType) {
		return motionType?.toLowerCase().trim() || "";
	}

	async function updatePictographData() {
		try {
			console.log("\u{1F50D} StaticSection: Updating pictograph data via static pictograph lookup...");

			const letter = (await $.track_reactivity_loss(findLetterForMotionParams()))();

			if (letter) {
				console.log(`\u{1F3AF} Found matching letter: ${letter}`);

				const gridMode = $.strict_equals($$props.motionState.gridType, "diamond") ? GridMode.DIAMOND : GridMode.BOX;

				(await $.track_reactivity_loss(pictographQueryService.initialize()))();

				const staticPictograph = (await $.track_reactivity_loss(pictographQueryService.getPictographByLetter(letter, gridMode)))();

				if (staticPictograph) {
					const positionedPictograph = (await $.track_reactivity_loss(arrowPositioningService.calculateAllArrowPositions(staticPictograph)))();

					$.set(pictographData, positionedPictograph, true);
					console.log("\u2705 StaticSection: Static pictograph data updated successfully");
				} else {
					console.warn(`\u26A0\uFE0F No static pictograph found for letter: ${letter}`);
					$.set(pictographData, null);
				}
			} else {
				console.warn("\u26A0\uFE0F No matching letter found for motion parameters");
				$.set(pictographData, null);
			}
		} catch(error) {
			console.error(...$.log_if_contains_state('error', "\u274C StaticSection: Error updating pictograph data:", error));
			$.set(pictographData, null);
		}
	}

	var div = root();
	var div_1 = $.child(div);
	var div_2 = $.child(div_1);
	var node = $.child(div_2);

	$.add_svelte_meta(
		() => SimpleGridToggle(node, {
			get state() {
				return $$props.motionState;
			}
		}),
		'component',
		StaticSection,
		142,
		6,
		{ componentTag: 'SimpleGridToggle' }
	);

	$.reset(div_2);

	var div_3 = $.sibling(div_2, 2);
	var node_1 = $.child(div_3);

	{
		var consequent = ($$anchor) => {
			var fragment = $.comment();
			var node_2 = $.first_child(fragment);

			$.add_svelte_meta(
				() => Pictograph(node_2, {
					get pictographData() {
						return $.get(pictographData);
					},

					width: PICTOGRAPH_SIZE,
					height: PICTOGRAPH_SIZE,
					debug: false
				}),
				'component',
				StaticSection,
				147,
				8,
				{ componentTag: 'Pictograph' }
			);

			$.append($$anchor, fragment);
		};

		var alternate = ($$anchor) => {
			var div_4 = root_2();

			$.append($$anchor, div_4);
		};

		$.add_svelte_meta(
			() => $.if(node_1, ($$render) => {
				if ($.get(pictographData)) $$render(consequent); else $$render(alternate, false);
			}),
			'if',
			StaticSection,
			146,
			6
		);
	}

	$.reset(div_3);
	$.reset(div_1);

	var div_5 = $.sibling(div_1, 2);
	var div_6 = $.child(div_5);
	var node_3 = $.child(div_6);

	$.add_svelte_meta(
		() => PropPanel(node_3, {
			propName: 'Blue',
			propColor: '#60a5fa',

			get startLocation() {
				return $$props.motionState.blueMotionParams.startLocation;
			},

			get endLocation() {
				return $$props.motionState.blueMotionParams.endLocation;
			},

			get startOrientation() {
				return $$props.motionState.blueMotionParams.startOrientation;
			},

			get endOrientation() {
				return $$props.motionState.blueMotionParams.endOrientation;
			},

			get turns() {
				return $$props.motionState.blueMotionParams.turns;
			},

			get motionType() {
				return $$props.motionState.blueMotionParams.motionType;
			},

			onStartLocationChange: (location) => $$props.motionState.setBlueStartLocation(location),
			onEndLocationChange: (location) => $$props.motionState.setBlueEndLocation(location),
			onStartOrientationChange: (orientation) => $$props.motionState.updateBlueMotionParam("startOrientation", orientation),
			onEndOrientationChange: (orientation) => $$props.motionState.updateBlueMotionParam("endOrientation", orientation),
			onTurnsChange: (turns) => $$props.motionState.updateBlueMotionParam("turns", turns),
			onMotionTypeChange: (motionType) => $$props.motionState.updateBlueMotionParam("motionType", motionType)
		}),
		'component',
		StaticSection,
		166,
		6,
		{ componentTag: 'PropPanel' }
	);

	$.reset(div_6);

	var div_7 = $.sibling(div_6, 2);
	var node_4 = $.child(div_7);

	$.add_svelte_meta(
		() => PropPanel(node_4, {
			propName: 'Red',
			propColor: '#f87171',

			get startLocation() {
				return $$props.motionState.redMotionParams.startLocation;
			},

			get endLocation() {
				return $$props.motionState.redMotionParams.endLocation;
			},

			get startOrientation() {
				return $$props.motionState.redMotionParams.startOrientation;
			},

			get endOrientation() {
				return $$props.motionState.redMotionParams.endOrientation;
			},

			get turns() {
				return $$props.motionState.redMotionParams.turns;
			},

			get motionType() {
				return $$props.motionState.redMotionParams.motionType;
			},

			onStartLocationChange: (location) => $$props.motionState.setRedStartLocation(location),
			onEndLocationChange: (location) => $$props.motionState.setRedEndLocation(location),
			onStartOrientationChange: (orientation) => $$props.motionState.updateRedMotionParam("startOrientation", orientation),
			onEndOrientationChange: (orientation) => $$props.motionState.updateRedMotionParam("endOrientation", orientation),
			onTurnsChange: (turns) => $$props.motionState.updateRedMotionParam("turns", turns),
			onMotionTypeChange: (motionType) => $$props.motionState.updateRedMotionParam("motionType", motionType)
		}),
		'component',
		StaticSection,
		194,
		6,
		{ componentTag: 'PropPanel' }
	);

	$.reset(div_7);
	$.reset(div_5);
	$.reset(div);
	$.append($$anchor, div);

	return $.pop({ ...$.legacy_api() });
}

if (import.meta.hot) {
	StaticSection = $.hmr(StaticSection, () => StaticSection[$.HMR].source);

	import.meta.hot.acceptExports(["default"],(module) => {
		$.cleanup_styles('s-aeptAB2-JEBZ');
		module.default[$.HMR].source = StaticSection[$.HMR].source;
		$.set(StaticSection[$.HMR].source, module.default[$.HMR].original);
	});
}

export default StaticSection;
