var root = $.add_locations($.from_html(`<div class="pictograph-renderer s-n855ULtPTM4D"><div class="grid-background s-n855ULtPTM4D"><!></div> <div class="pictograph-content s-n855ULtPTM4D"><!> <!> <!> <!></div> <!></div>`), PictographRenderer[$.FILENAME], [[74, 0, [[79, 2], [140, 2]]]]);

function PictographRenderer($$anchor, $$props) {
	$.check_target(new.target);
	$.push($$props, true, PictographRenderer);

	let size = $.prop($$props, 'size', 3, "medium"),
		showDetails = $.prop($$props, 'showDetails', 3, false);

	let gridSize = $.tag($.derived(() => getGridSize(size())), 'gridSize');
	let positions = $.tag($.derived(() => getPositions($$props.pictographData)), 'positions');

	function getGridSize(size2) {
		switch (size2) {
			case "small":
				return 80;

			case "medium":
				return 120;

			case "large":
				return 160;

			default:
				return 120;
		}
	}

	function getPositions(data) {
		if (!data) return { start: null, end: null };

		return {
			start: parsePosition(data.startPosition || ""),
			end: parsePosition(data.endPosition || "")
		};
	}

	function parsePosition(pos) {
		if (!pos) return null;

		const positionMap = {
			n: { x: 0.5, y: 0 },
			ne: { x: 1, y: 0 },
			e: { x: 1, y: 0.5 },
			se: { x: 1, y: 1 },
			s: { x: 0.5, y: 1 },
			sw: { x: 0, y: 1 },
			w: { x: 0, y: 0.5 },
			nw: { x: 0, y: 0 }
		};

		return positionMap[pos.toLowerCase()] || { x: 0.5, y: 0.5 };
	}

	function getPositionStyle(position, _isStart = true) {
		if (!position) return "";

		const x = position.x * $.get(gridSize);
		const y = position.y * $.get(gridSize);

		return `left: ${x}px; top: ${y}px;`;
	}

	function getLineStyle() {
		if (!$.get(positions).start || !$.get(positions).end) return "";

		const startX = $.get(positions).start.x * $.get(gridSize);
		const startY = $.get(positions).start.y * $.get(gridSize);
		const endX = $.get(positions).end.x * $.get(gridSize);
		const endY = $.get(positions).end.y * $.get(gridSize);
		const deltaX = endX - startX;
		const deltaY = endY - startY;
		const length = Math.sqrt(deltaX * deltaX + deltaY * deltaY);
		const angle = Math.atan2(deltaY, deltaX) * (180 / Math.PI);

		return `
			left: ${startX}px;
			top: ${startY}px;
			width: ${length}px;
			transform: rotate(${angle}deg);
			transform-origin: 0 50%;
		`;
	}

	var div = root();
	var div_1 = $.child(div);
	var node = $.child(div_1);

	{
		var consequent = ($$anchor) => {
			var svg = root_1();
			var path = $.child(svg);
			var line = $.sibling(path);
			var line_1 = $.sibling(line);

			$.reset(svg);

			$.template_effect(() => {
				$.set_attribute(svg, 'width', $.get(gridSize));
				$.set_attribute(svg, 'height', $.get(gridSize));
				$.set_attribute(path, 'd', `M ${$.get(gridSize) / 2} 0 L ${$.get(gridSize) ?? ''} ${$.get(gridSize) / 2} L ${$.get(gridSize) / 2} ${$.get(gridSize) ?? ''} L 0 ${$.get(gridSize) / 2} Z`);
				$.set_attribute(line, 'x1', $.get(gridSize) / 2);
				$.set_attribute(line, 'x2', $.get(gridSize) / 2);
				$.set_attribute(line, 'y2', $.get(gridSize));
				$.set_attribute(line_1, 'y1', $.get(gridSize) / 2);
				$.set_attribute(line_1, 'x2', $.get(gridSize));
				$.set_attribute(line_1, 'y2', $.get(gridSize) / 2);
			});

			$.append($$anchor, svg);
		};

		var alternate = ($$anchor) => {
			var svg_1 = root_2();

			$.template_effect(() => {
				$.set_attribute(svg_1, 'width', $.get(gridSize));
				$.set_attribute(svg_1, 'height', $.get(gridSize));
			});

			$.append($$anchor, svg_1);
		};

		$.add_svelte_meta(
			() => $.if(node, ($$render) => {
				if ($.strict_equals($$props.pictographData?.gridMode, "diamond")) $$render(consequent); else $$render(alternate, false);
			}),
			'if',
			PictographRenderer,
			80,
			4
		);
	}

	$.reset(div_1);

	var div_2 = $.sibling(div_1, 2);
	var node_1 = $.child(div_2);

	{
		var consequent_1 = ($$anchor) => {
			var div_3 = root_3();

			$.template_effect(($0) => $.set_style(div_3, $0), [() => getPositionStyle($.get(positions).start, true)]);
			$.append($$anchor, div_3);
		};

		$.add_svelte_meta(
			() => $.if(node_1, ($$render) => {
				if ($.get(positions).start) $$render(consequent_1);
			}),
			'if',
			PictographRenderer,
			142,
			4
		);
	}

	var node_2 = $.sibling(node_1, 2);

	{
		var consequent_2 = ($$anchor) => {
			var div_4 = root_4();

			$.template_effect(($0) => $.set_style(div_4, $0), [() => getPositionStyle($.get(positions).end, false)]);
			$.append($$anchor, div_4);
		};

		$.add_svelte_meta(
			() => $.if(node_2, ($$render) => {
				if ($.get(positions).end) $$render(consequent_2);
			}),
			'if',
			PictographRenderer,
			153,
			4
		);
	}

	var node_3 = $.sibling(node_2, 2);

	{
		var consequent_3 = ($$anchor) => {
			var div_5 = root_5();

			$.template_effect(($0) => $.set_style(div_5, $0), [getLineStyle]);
			$.append($$anchor, div_5);
		};

		$.add_svelte_meta(
			() => $.if(node_3, ($$render) => {
				if ($.get(positions).start && $.get(positions).end) $$render(consequent_3);
			}),
			'if',
			PictographRenderer,
			164,
			4
		);
	}

	var node_4 = $.sibling(node_3, 2);

	{
		var consequent_4 = ($$anchor) => {
			var div_6 = root_6();
			var text = $.child(div_6, true);

			$.reset(div_6);
			$.template_effect(() => $.set_text(text, $$props.pictographData.letter));
			$.append($$anchor, div_6);
		};

		$.add_svelte_meta(
			() => $.if(node_4, ($$render) => {
				if ($$props.pictographData?.letter) $$render(consequent_4);
			}),
			'if',
			PictographRenderer,
			169,
			4
		);
	}

	$.reset(div_2);

	var node_5 = $.sibling(div_2, 2);

	{
		var consequent_5 = ($$anchor) => {
			var div_7 = root_7();
			var div_8 = $.child(div_7);
			var span = $.sibling($.child(div_8), 2);
			var text_1 = $.child(span, true);

			$.reset(span);
			$.reset(div_8);

			var div_9 = $.sibling(div_8, 2);
			var span_1 = $.sibling($.child(div_9), 2);
			var text_2 = $.child(span_1, true);

			$.reset(span_1);
			$.reset(div_9);

			var div_10 = $.sibling(div_9, 2);
			var span_2 = $.sibling($.child(div_10), 2);
			var text_3 = $.child(span_2, true);

			$.reset(span_2);
			$.reset(div_10);

			var div_11 = $.sibling(div_10, 2);
			var span_3 = $.sibling($.child(div_11), 2);
			var text_4 = $.child(span_3, true);

			$.reset(span_3);
			$.reset(div_11);
			$.reset(div_7);

			$.template_effect(() => {
				$.set_text(text_1, $$props.pictographData.letter);
				$.set_text(text_2, $$props.pictographData.startPosition);
				$.set_text(text_3, $$props.pictographData.endPosition);
				$.set_text(text_4, $$props.pictographData.gridMode);
			});

			$.append($$anchor, div_7);
		};

		$.add_svelte_meta(
			() => $.if(node_5, ($$render) => {
				if (showDetails() && $$props.pictographData) $$render(consequent_5);
			}),
			'if',
			PictographRenderer,
			177,
			2
		);
	}

	$.reset(div);
	$.template_effect(() => $.set_style(div, `width: ${$.get(gridSize) ?? ''}px; height: ${$.get(gridSize) ?? ''}px;`));
	$.append($$anchor, div);

	return $.pop({ ...$.legacy_api() });
}

if (import.meta.hot) {
	PictographRenderer = $.hmr(PictographRenderer, () => PictographRenderer[$.HMR].source);

	import.meta.hot.acceptExports(["default"],(module) => {
		$.cleanup_styles('s-n855ULtPTM4D');
		module.default[$.HMR].source = PictographRenderer[$.HMR].source;
		$.set(PictographRenderer[$.HMR].source, module.default[$.HMR].original);
	});
}

export default PictographRenderer;
