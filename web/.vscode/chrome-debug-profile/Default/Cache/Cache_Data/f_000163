var root_3 = $.add_locations($.from_html(`<div class="position-marker start-position s-n855ULtPTM4D"><div class="marker-dot s-n855ULtPTM4D"></div> <div class="marker-label s-n855ULtPTM4D">Start</div></div>`), PictographRenderer[$.FILENAME], [[143, 6, [[147, 8], [148, 8]]]]);
var root_4 = $.add_locations($.from_html(`<div class="position-marker end-position s-n855ULtPTM4D"><div class="marker-dot s-n855ULtPTM4D"></div> <div class="marker-label s-n855ULtPTM4D">End</div></div>`), PictographRenderer[$.FILENAME], [[154, 6, [[158, 8], [159, 8]]]]);
var root_5 = $.add_locations($.from_html(`<div class="connection-line s-n855ULtPTM4D"></div>`), PictographRenderer[$.FILENAME], [[165, 6]]);
var root_6 = $.add_locations($.from_html(`<div class="letter-overlay s-n855ULtPTM4D"> </div>`), PictographRenderer[$.FILENAME], [[170, 6]]);

var root_7 = $.add_locations($.from_html(`<div class="details-panel s-n855ULtPTM4D"><div class="detail-item s-n855ULtPTM4D"><span class="label s-n855ULtPTM4D">Letter:</span> <span class="value s-n855ULtPTM4D"> </span></div> <div class="detail-item s-n855ULtPTM4D"><span class="label s-n855ULtPTM4D">Start:</span> <span class="value s-n855ULtPTM4D"> </span></div> <div class="detail-item s-n855ULtPTM4D"><span class="label s-n855ULtPTM4D">End:</span> <span class="value s-n855ULtPTM4D"> </span></div> <div class="detail-item s-n855ULtPTM4D"><span class="label s-n855ULtPTM4D">Grid:</span> <span class="value s-n855ULtPTM4D"> </span></div></div>`), PictographRenderer[$.FILENAME], [
