PictographRenderer[$.FILENAME] = 'src/lib/components/learn/PictographRenderer.svelte';

import * as $ from "/node_modules/.vite/deps/svelte_internal_client.js?v=de368f3a";

var root_1 = $.add_locations($.from_svg(`<svg class="grid-svg s-n855ULtPTM4D"><path fill="none" stroke="rgba(255, 255, 255, 0.2)" stroke-width="1" class="s-n855ULtPTM4D"></path><line y1="0" stroke="rgba(255, 255, 255, 0.1)" stroke-width="1" class="s-n855ULtPTM4D"></line><line x1="0" stroke="rgba(255, 255, 255, 0.1)" stroke-width="1" class="s-n855ULtPTM4D"></line></svg>`), PictographRenderer[$.FILENAME], [[81, 6, [[83, 8], [91, 8], [99, 8]]]]);

var root_2 = $.add_locations($.from_svg(`<svg class="grid-svg s-n855ULtPTM4D"><defs class="s-n855ULtPTM4D"><pattern id="grid" width="20" height="20" patternUnits="userSpaceOnUse" class="s-n855ULtPTM4D"><path d="M 20 0 L 0 0 0 20" fill="none" stroke="rgba(255, 255, 255, 0.1)" stroke-width="1" class="s-n855ULtPTM4D"></path></pattern></defs><rect width="100%" height="100%" fill="url(#grid)" class="s-n855ULtPTM4D"></rect><rect width="100%" height="100%" fill="none" stroke="rgba(255, 255, 255, 0.2)" stroke-width="2" class="s-n855ULtPTM4D"></rect></svg>`), PictographRenderer[$.FILENAME], [
