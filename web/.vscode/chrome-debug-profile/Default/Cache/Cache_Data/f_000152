import { createHotContext as __vite__createHotContext } from "/@vite/client";import.meta.hot = __vite__createHotContext("/src/lib/components/ui/Splitter.svelte");import "/node_modules/.vite/deps/svelte_internal_disclose-version.js?v=de368f3a";

Splitter[$.FILENAME] = 'src/lib/components/ui/Splitter.svelte';

import * as $ from "/node_modules/.vite/deps/svelte_internal_client.js?v=de368f3a";
import { onDestroy } from "/node_modules/.vite/deps/svelte.js?v=de368f3a";

function handleMouseDown(
	event,
	disabled,
	isDragging,
	startX,
	startY,
	handleMouseMove,
	handleMouseUp,
	direction,
	onResizeStart
) {
	if (disabled()) return;

	event.preventDefault();
	event.stopPropagation();
	$.set(isDragging, true);
	$.set(startX, event.clientX, true);
	$.set(startY, event.clientY, true);
	document.addEventListener("mousemove", handleMouseMove);
	document.addEventListener("mouseup", handleMouseUp);
	document.body.style.cursor = $.strict_equals(direction(), "right") ? "col-resize" : "col-resize";
	document.body.style.userSelect = "none";
	onResizeStart()($.get(startX));
}

function handleTouchStart(
	event,
	disabled,
	isDragging,
	startX,
	startY,
	handleTouchMove,
	handleTouchEnd,
	onResizeStart
) {
	if (disabled()) return;

	event.preventDefault();

	const touch = event.touches[0];

	if (!touch) return;

	$.set(isDragging, true);
	$.set(startX, touch.clientX, true);
	$.set(startY, touch.clientY, true);
	document.addEventListener("touchmove", handleTouchMove, { passive: false });
	document.addEventListener("touchend", handleTouchEnd);
	onResizeStart()($.get(startX));
}

var on_keydown = (e, disabled, onResizeMove) => {
	// Allow keyboard resizing with arrow keys
	if (disabled()) return;

	if ($.strict_equals(e.key, "ArrowLeft")) {
		onResizeMove()(-10);
	} else if ($.strict_equals(e.key, "ArrowRight")) {
		onResizeMove()(10);
	}
};

var root = $.add_locations($.from_html(`<div role="slider" aria-label="Resize panel" aria-valuemin="10" aria-valuemax="90" aria-valuenow="50"><div class="splitter-handle s-KKbOAUfezSGk"><div class="splitter-grip s-KKbOAUfezSGk"><div class="grip-dot s-KKbOAUfezSGk"></div> <div class="grip-dot s-KKbOAUfezSGk"></div> <div class="grip-dot s-KKbOAUfezSGk"></div></div></div></div>`), Splitter[$.FILENAME], [
	[
		91,
		0,
		[[117, 2, [[118, 4, [[119, 6], [120, 6], [121, 6]]]]]]
	]
]);

function Splitter($$anchor, $$props) {
	$.check_target(new.target);
	$.push($$props, true, Splitter);

	const direction = $.prop($$props, 'direction', 3, "right"),
		disabled = $.prop($$props, 'disabled', 3, false),
		thickness = $.prop($$props, 'thickness', 3, 8),
		onResizeStart = $.prop($$props, 'onResizeStart', 3, () => {}),
		onResizeMove = $.prop($$props, 'onResizeMove', 3, (deltaX) => {}),
		onResizeEnd = $.prop($$props, 'onResizeEnd', 3, () => {});

	let isDragging = $.tag($.state(false), 'isDragging');
	let startX = $.tag($.state(0), 'startX');
	let startY = $.tag($.state(0), 'startY');

	function handleMouseMove(event) {
		if (!$.get(isDragging)) return;

		event.preventDefault();

		const currentX = event.clientX;
		const deltaX = currentX - $.get(startX);

		onResizeMove()(deltaX);
	}

	function handleMouseUp(event) {
		if (!$.get(isDragging)) return;

		$.set(isDragging, false);
		document.removeEventListener("mousemove", handleMouseMove);
		document.removeEventListener("mouseup", handleMouseUp);
		document.body.style.cursor = "";
		document.body.style.userSelect = "";
		onResizeEnd()();
	}

	function handleTouchMove(event) {
		if (!$.get(isDragging)) return;

		event.preventDefault();

		const touch = event.touches[0];

		if (!touch) return;

		const currentX = touch.clientX;
		const deltaX = currentX - $.get(startX);

		onResizeMove()(deltaX);
	}

	function handleTouchEnd(event) {
		if (!$.get(isDragging)) return;

		$.set(isDragging, false);
		document.removeEventListener("touchmove", handleTouchMove);
		document.removeEventListener("touchend", handleTouchEnd);
		onResizeEnd()();
	}

	onDestroy(() => {
		if ($.get(isDragging)) {
			document.removeEventListener("mousemove", handleMouseMove);
			document.removeEventListener("mouseup", handleMouseUp);
			document.removeEventListener("touchmove", handleTouchMove);
			document.removeEventListener("touchend", handleTouchEnd);
			document.body.style.cursor = "";
			document.body.style.userSelect = "";
		}
	});

	var div = root();
	let classes;

	div.__mousedown = [
		handleMouseDown,
		disabled,
		isDragging,
		startX,
		startY,
		handleMouseMove,
		handleMouseUp,
		direction,
		onResizeStart
	];

	div.__touchstart = [
		handleTouchStart,
		disabled,
		isDragging,
		startX,
		startY,
		handleTouchMove,
		handleTouchEnd,
		onResizeStart
	];

	div.__keydown = [on_keydown, disabled, onResizeMove];

	$.template_effect(
		($0) => {
			classes = $.set_class(div, 1, 'splitter s-KKbOAUfezSGk', null, classes, $0);
			$.set_style(div, `width: ${thickness() ?? ''}px;`);
			$.set_attribute(div, 'tabindex', disabled() ? -1 : 0);
		},
		[
			() => ({
				disabled: disabled(),
				dragging: $.get(isDragging),
				'left-direction': $.strict_equals(direction(), "left"),
				'right-direction': $.strict_equals(direction(), "right")
			})
		]
	);

	$.append($$anchor, div);

	return $.pop({ ...$.legacy_api() });
}

if (import.meta.hot) {
	Splitter = $.hmr(Splitter, () => Splitter[$.HMR].source);

	import.meta.hot.acceptExports(["default"],(module) => {
		$.cleanup_styles('s-KKbOAUfezSGk');
		module.default[$.HMR].source = Splitter[$.HMR].source;
		$.set(Splitter[$.HMR].source, module.default[$.HMR].original);
	});
}

export default Splitter;

$.delegate(['mousedown', 'touchstart', 'keydown']);
import "/src/lib/components/ui/Splitter.svelte?svelte&type=style&lang.css";

