import { createHotContext as __vite__createHotContext } from "/@vite/client";import.meta.hot = __vite__createHotContext("/src/lib/components/settings/tabs/CodexExporterTab.svelte");import "/node_modules/.vite/deps/svelte_internal_disclose-version.js?v=474c1bba";

CodexExporterTab[$.FILENAME] = 'src/lib/components/settings/tabs/CodexExporterTab.svelte';

import * as $ from "/node_modules/.vite/deps/svelte_internal_client.js?v=474c1bba";
import SelectInput from "/src/lib/components/settings/SelectInput.svelte";
import SettingCard from "/src/lib/components/settings/SettingCard.svelte";
import TextInput from "/src/lib/components/settings/TextInput.svelte";
import ToggleSetting from "/src/lib/components/settings/ToggleSetting.svelte";

function handleExportClick(_, $$props) {
	$$props.onExport?.();
	alert("Pictograph export has been initiated. This may take several minutes to complete.");
}

var root_1 = $.add_locations($.from_html(`<div class="section-group s-6ugFMSabBVin"><h4 class="section-title s-6ugFMSabBVin">Turn Configuration</h4> <div class="turn-inputs s-6ugFMSabBVin"><!> <!></div></div> <div class="section-group s-6ugFMSabBVin"><h4 class="section-title s-6ugFMSabBVin">Grid Mode</h4> <!></div> <div class="section-group s-6ugFMSabBVin"><h4 class="section-title s-6ugFMSabBVin">Export Options</h4> <!> <!> <!></div> <div class="export-section s-6ugFMSabBVin"><button class="export-button s-6ugFMSabBVin">Export All Pictographs</button></div>`, 1), CodexExporterTab[$.FILENAME], [
	[72, 4, [[73, 6], [74, 6]]],
	[97, 4, [[98, 6]]],
	[109, 4, [[110, 6]]],
	[137, 4, [[138, 6]]]
]);

var root = $.add_locations($.from_html(`<div class="tab-content s-6ugFMSabBVin"><!></div>`), CodexExporterTab[$.FILENAME], [[66, 0]]);

function CodexExporterTab($$anchor, $$props) {
	$.check_target(new.target);
	$.push($$props, true, CodexExporterTab);

	let redTurns = $.tag($.state($.proxy($$props.settings.codexExporter?.redTurns ?? 16)), 'redTurns');
	let blueTurns = $.tag($.state($.proxy($$props.settings.codexExporter?.blueTurns ?? 16)), 'blueTurns');
	let gridMode = $.tag($.state($.proxy($$props.settings.codexExporter?.gridMode ?? GridMode.DIAMOND)), 'gridMode');
	let generateAll = $.tag($.state($.proxy($$props.settings.codexExporter?.generateAll ?? true)), 'generateAll');
	let quality = $.tag($.state($.proxy($$props.settings.codexExporter?.quality ?? 300)), 'quality');
	let includeMetadata = $.tag($.state($.proxy($$props.settings.codexExporter?.includeMetadata ?? true)), 'includeMetadata');

	const gridModeOptions = [
		{ value: GridMode.DIAMOND, label: "Diamond" },
		{ value: GridMode.BOX, label: "Box" }
	];

	function handleRedTurnsChange(event) {
		$.set(redTurns, parseInt(event.detail), true);
		saveSettings();
	}

	function handleBlueTurnsChange(event) {
		$.set(blueTurns, parseInt(event.detail), true);
		saveSettings();
	}

	function handleGridModeChange(event) {
		$.set(gridMode, event.detail, true);
		saveSettings();
	}

	function handleGenerateAllChange(event) {
		$.set(generateAll, event.detail, true);
		saveSettings();
	}

	function handleQualityChange(event) {
		$.set(quality, parseInt(event.detail), true);
		saveSettings();
	}

	function handleIncludeMetadataChange(event) {
		$.set(includeMetadata, event.detail, true);
		saveSettings();
	}

	function saveSettings() {
		const codexExporterSettings = {
			redTurns: $.get(redTurns),
			blueTurns: $.get(blueTurns),
			gridMode: $.get(gridMode),
			generateAll: $.get(generateAll),
			quality: $.get(quality),
			includeMetadata: $.get(includeMetadata)
		};

		$$props.onUpdate?.({ key: "codexExporter", value: codexExporterSettings });
	}

	var div = root();
	var node = $.child(div);

	$.add_svelte_meta(
		() => SettingCard(node, {
			title: 'Codex Exporter',
			description: 'Export all pictographs with customizable turn configurations',

			children: $.wrap_snippet(CodexExporterTab, ($$anchor, $$slotProps) => {
				var fragment = root_1();
				var div_1 = $.first_child(fragment);
				var div_2 = $.sibling($.child(div_1), 2);
				var node_1 = $.child(div_2);

				{
					let $0 = $.derived(() => $.get(redTurns).toString());

					$.add_svelte_meta(
						() => TextInput(node_1, {
							label: 'Red Turns',

							get value() {
								return $.get($0);
							},

							type: 'number',
							min: 1,
							max: 64,
							helpText: 'Number of red turns for pictographs',
							$$events: { change: handleRedTurnsChange }
						}),
						'component',
						CodexExporterTab,
						75,
						8,
						{ componentTag: 'TextInput' }
					);
				}

				var node_2 = $.sibling(node_1, 2);

				{
					let $0 = $.derived(() => $.get(blueTurns).toString());

					$.add_svelte_meta(
						() => TextInput(node_2, {
							label: 'Blue Turns',

							get value() {
								return $.get($0);
							},

							type: 'number',
							min: 1,
							max: 64,
							helpText: 'Number of blue turns for pictographs',
							$$events: { change: handleBlueTurnsChange }
						}),
						'component',
						CodexExporterTab,
						84,
						8,
						{ componentTag: 'TextInput' }
					);
				}

				$.reset(div_2);
				$.reset(div_1);

				var div_3 = $.sibling(div_1, 2);
				var node_3 = $.sibling($.child(div_3), 2);

				$.add_svelte_meta(
					() => SelectInput(node_3, {
						label: 'Grid Layout',

						get value() {
							return $.get(gridMode);
						},

						get options() {
							return gridModeOptions;
						},

						helpText: 'Pictograph grid layout style',
						$$events: { change: handleGridModeChange }
					}),
					'component',
					CodexExporterTab,
					99,
					6,
					{ componentTag: 'SelectInput' }
				);

				$.reset(div_3);

				var div_4 = $.sibling(div_3, 2);
				var node_4 = $.sibling($.child(div_4), 2);

				$.add_svelte_meta(
					() => ToggleSetting(node_4, {
						label: 'Generate All Variations',

						get checked() {
							return $.get(generateAll);
						},

						helpText: 'Generate all possible pictograph variations',
						$$events: { change: handleGenerateAllChange }
					}),
					'component',
					CodexExporterTab,
					111,
					6,
					{ componentTag: 'ToggleSetting' }
				);

				var node_5 = $.sibling(node_4, 2);

				{
					let $0 = $.derived(() => $.get(quality).toString());

					$.add_svelte_meta(
						() => TextInput(node_5, {
							label: 'Image Quality (DPI)',

							get value() {
								return $.get($0);
							},

							type: 'number',
							min: 72,
							max: 600,
							helpText: 'Image resolution for exported pictographs',
							$$events: { change: handleQualityChange }
						}),
						'component',
						CodexExporterTab,
						118,
						6,
						{ componentTag: 'TextInput' }
					);
				}

				var node_6 = $.sibling(node_5, 2);

				$.add_svelte_meta(
					() => ToggleSetting(node_6, {
						label: 'Include Export Metadata',

						get checked() {
							return $.get(includeMetadata);
						},

						helpText: 'Include metadata information in exported files',
						$$events: { change: handleIncludeMetadataChange }
					}),
					'component',
					CodexExporterTab,
					128,
					6,
					{ componentTag: 'ToggleSetting' }
				);

				$.reset(div_4);

				var div_5 = $.sibling(div_4, 2);
				var button = $.child(div_5);

				button.__click = [handleExportClick, $$props];
				$.reset(div_5);
				$.append($$anchor, fragment);
			}),

			$$slots: { default: true }
		}),
		'component',
		CodexExporterTab,
		67,
		2,
		{ componentTag: 'SettingCard' }
	);

	$.reset(div);
	$.append($$anchor, div);

	return $.pop({ ...$.legacy_api() });
}

if (import.meta.hot) {
	CodexExporterTab = $.hmr(CodexExporterTab, () => CodexExporterTab[$.HMR].source);

	import.meta.hot.acceptExports(["default"],(module) => {
		$.cleanup_styles('s-6ugFMSabBVin');
		module.default[$.HMR].source = CodexExporterTab[$.HMR].source;
		$.set(CodexExporterTab[$.HMR].source, module.default[$.HMR].original);
	});
}

export default CodexExporterTab;

$.delegate(['click']);
import "/src/lib/components/settings/tabs/CodexExporterTab.svelte?svelte&type=style&lang.css";

