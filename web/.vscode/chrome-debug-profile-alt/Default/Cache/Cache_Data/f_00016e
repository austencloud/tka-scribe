											$.template_effect(() => $.set_text(text_2, $$props.arrowData?.arrowLocation));
											$.append($$anchor, fragment_6);
										};

										$.add_svelte_meta(
											() => $.if(node_4, ($$render) => {
												if (import.meta.env.DEV) $$render(consequent_4);
											}),
											'if',
											ArrowSvg,
											130,
