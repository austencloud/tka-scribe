							var fragment_4 = $.comment();
							var node_3 = $.first_child(fragment_4);

							{
								var consequent_3 = ($$anchor) => {
									var g_1 = root_7();
									let classes_1;
									let styles;
									var image = $.child(g_1);

									$.reset(g_1);

									$.template_effect(
										($0, $1, $2, $3, $4, $5) => {
											$.set_attribute(g_1, 'transform', `
        translate(${$0 ?? ''}, ${$1 ?? ''})
        rotate(${$2 ?? ''})
        scale(${$3 ?? ''}, 1)
        translate(${-$.get(svgData).center.x}, ${-$.get(svgData).center.y})
      `);

											classes_1 = $.set_class(g_1, 0, `arrow-svg ${$$props.motionData?.color ?? ''}-arrow-svg`, 's-DGO6GOA5RSra', classes_1, $4);
											styles = $.set_style(g_1, '', styles, $5);
											$.set_attribute(image, 'href', $.get(svgData).imageSrc);
											$.set_attribute(image, 'width', $.get(svgData).viewBox.width);
											$.set_attribute(image, 'height', $.get(svgData).viewBox.height);
										},
										[
											() => $.get(calculatedPosition)().x,
											() => $.get(calculatedPosition)().y,
											() => $.get(calculatedPosition)().rotation || $$props.arrowData?.rotationAngle || 0,
