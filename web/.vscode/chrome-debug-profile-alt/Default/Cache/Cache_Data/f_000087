import { createHotContext as __vite__createHotContext } from "/@vite/client";import.meta.hot = __vite__createHotContext("/src/lib/components/learn/QuizTimer.svelte");import "/node_modules/.vite/deps/svelte_internal_disclose-version.js?v=474c1bba";

QuizTimer[$.FILENAME] = 'src/lib/components/learn/QuizTimer.svelte';

import * as $ from "/node_modules/.vite/deps/svelte_internal_client.js?v=474c1bba";
import { onDestroy, onMount } from "/node_modules/.vite/deps/svelte.js?v=474c1bba";

var root_1 = $.add_locations($.from_html(`<span class="status-text s-QAs0hLEQ3Lf6">PAUSED</span>`), QuizTimer[$.FILENAME], [[158, 8]]);
var root_3 = $.add_locations($.from_html(`<span class="status-text s-QAs0hLEQ3Lf6">STOPPED</span>`), QuizTimer[$.FILENAME], [[160, 8]]);

var root = $.add_locations($.from_html(`<div><div class="timer-ring s-QAs0hLEQ3Lf6"><svg class="progress-ring s-QAs0hLEQ3Lf6" viewBox="0 0 120 120"><circle class="progress-ring-background s-QAs0hLEQ3Lf6" cx="60" cy="60" r="54" fill="transparent" stroke="rgba(255, 255, 255, 0.1)" stroke-width="4"></circle><circle class="progress-ring-progress s-QAs0hLEQ3Lf6" cx="60" cy="60" r="54" fill="transparent" stroke-width="4" stroke-dasharray="339.292" transform="rotate(-90 60 60)"></circle></svg> <div class="time-display s-QAs0hLEQ3Lf6"><span class="time-text s-QAs0hLEQ3Lf6"> </span> <!></div></div> <!></div>`), QuizTimer[$.FILENAME], [
	[
		128,
		0,

		[
			[
				130,
				2,
				[[131, 4, [[132, 6], [141, 6]]], [155, 4, [[156, 6]]]]
			]
		]
	]
]);

function QuizTimer($$anchor, $$props) {
	$.check_target(new.target);
	$.push($$props, true, QuizTimer);

	let timeRemaining = $.prop($$props, 'timeRemaining', 7, 120),
		totalTime = $.prop($$props, 'totalTime', 7, 120),
		isRunning = $.prop($$props, 'isRunning', 7, false),
		isPaused = $.prop($$props, 'isPaused', 7, false),
		showWarnings = $.prop($$props, 'showWarnings', 3, true),
		size = $.prop($$props, 'size', 3, "medium");

	let interval = null;
	let lastWarningTime = 0;
	let formattedTime = $.tag($.derived(() => formatTime(timeRemaining())), 'formattedTime');
	let progressPercentage = $.tag($.derived(() => totalTime() > 0 ? timeRemaining() / totalTime() * 100 : 0), 'progressPercentage');
	let timerClass = $.tag($.derived(getTimerClass), 'timerClass');
	let isWarning = $.tag($.derived(() => timeRemaining() <= 30 && timeRemaining() > 10), 'isWarning');
	let isCritical = $.tag($.derived(() => timeRemaining() <= 10), 'isCritical');

	onMount(() => {
		if (isRunning()) {
			startTimer();
		}
	});

	onDestroy(() => {
		stopTimer();
	});

	$.user_effect(() => {
		if (isRunning() && !interval) {
			startTimer();
		} else if (!isRunning() && interval) {
			stopTimer();
		}
	});

	function startTimer() {
		if (interval) return;

		interval = setInterval(
			() => {
				if (timeRemaining() > 0) {
					$.update_prop(timeRemaining, -1);
					$$props.ontick?.({ timeRemaining: timeRemaining() });

					if (showWarnings()) {
						checkWarnings();
					}

					if (timeRemaining() <= 0) {
						$$props.ontimeup?.();
						stopTimer();
					}
				}
			},
			1e3
		);
	}

	function stopTimer() {
		if (interval) {
			clearInterval(interval);
			interval = null;
		}
	}

	function checkWarnings() {
		const warningTimes = [60, 30, 10, 5];

		for (const warningTime of warningTimes) {
			if ($.strict_equals(timeRemaining(), warningTime) && $.strict_equals(lastWarningTime, warningTime, false)) {
				lastWarningTime = warningTime;
				$$props.onwarning?.({ timeRemaining: timeRemaining() });

				break;
			}
		}
	}

	function formatTime(seconds) {
		const minutes = Math.floor(seconds / 60);
		const remainingSeconds = seconds % 60;

		return `${minutes}:${remainingSeconds.toString().padStart(2, "0")}`;
	}

	function getTimerClass() {
		let classes = ["quiz-timer", `size-${size()}`];

		if ($.get(isCritical)) {
			classes.push("critical");
		} else if ($.get(isWarning)) {
			classes.push("warning");
		}

		if (isPaused()) {
			classes.push("paused");
		}

		if (!isRunning()) {
			classes.push("stopped");
		}

		return classes.join(" ");
	}

	function start() {
		isRunning(true);
		startTimer();
	}

	function pause() {
		isPaused(true);
		isRunning(false);
		stopTimer();
	}

	function resume() {
		isPaused(false);
		isRunning(true);
		startTimer();
	}

	function stop() {
		isRunning(false);
		isPaused(false);
		stopTimer();
	}

	function reset(newTime) {
		stop();

		if ($.strict_equals(newTime, void 0, false)) {
			timeRemaining(newTime);
			totalTime(newTime);
		} else {
			timeRemaining(totalTime());
		}

		lastWarningTime = 0;
	}

	var div = root();
	var div_1 = $.child(div);
	var svg = $.child(div_1);
	var circle = $.sibling($.child(svg));

	$.reset(svg);

	var div_2 = $.sibling(svg, 2);
	var span = $.child(div_2);
	var text = $.child(span, true);

	$.reset(span);

	var node = $.sibling(span, 2);

	{
		var consequent = ($$anchor) => {
			var span_1 = root_1();

			$.append($$anchor, span_1);
		};

		var alternate = ($$anchor) => {
			var fragment = $.comment();
			var node_1 = $.first_child(fragment);

			{
				var consequent_1 = ($$anchor) => {
					var span_2 = root_3();

					$.append($$anchor, span_2);
				};

				$.add_svelte_meta(
					() => $.if(
						node_1,
						($$render) => {
							if (!isRunning()) $$render(consequent_1);
						},
						true
					),
					'if',
					QuizTimer,
					159,
					6
				);
			}

			$.append($$anchor, fragment);
		};

		$.add_svelte_meta(
			() => $.if(node, ($$render) => {
				if (isPaused()) $$render(consequent); else $$render(alternate, false);
			}),
			'if',
			QuizTimer,
			157,
			6
		);
	}

	$.reset(div_2);
	$.reset(div_1);

	var node_2 = $.sibling(div_1, 2);

	{
		var consequent_2 = ($$anchor) => {
			var fragment_1 = $.comment();
			var node_3 = $.first_child(fragment_1);

			$.add_svelte_meta(() => $.snippet(node_3, () => $$props.controls), 'render', QuizTimer, 167, 4);
			$.append($$anchor, fragment_1);
		};

		$.add_svelte_meta(
			() => $.if(node_2, ($$render) => {
				if ($$props.controls) $$render(consequent_2);
			}),
			'if',
			QuizTimer,
			166,
			2
		);
	}

	$.reset(div);

	$.template_effect(() => {
		$.set_class(div, 1, $.clsx($.get(timerClass)), 's-QAs0hLEQ3Lf6');
		$.set_attribute(circle, 'stroke-dashoffset', 339.292 - $.get(progressPercentage) / 100 * 339.292);
		$.set_text(text, $.get(formattedTime));
	});

	$.append($$anchor, div);

	return $.pop({
		get start() {
			return start;
		},

		get pause() {
			return pause;
		},

		get resume() {
			return resume;
		},

		get stop() {
			return stop;
		},

		get reset() {
			return reset;
		},

		...$.legacy_api()
	});
}

if (import.meta.hot) {
	QuizTimer = $.hmr(QuizTimer, () => QuizTimer[$.HMR].source);

	import.meta.hot.acceptExports(["default"],(module) => {
		$.cleanup_styles('s-QAs0hLEQ3Lf6');
		module.default[$.HMR].source = QuizTimer[$.HMR].source;
		$.set(QuizTimer[$.HMR].source, module.default[$.HMR].original);
	});
}

export default QuizTimer;
import "/src/lib/components/learn/QuizTimer.svelte?svelte&type=style&lang.css";

