function handleResizeStart(event, isResizing, codexWidth) {
	$.set(isResizing, true);
	event.preventDefault();

	const startX = event.clientX;
	const startWidth = $.get(codexWidth);
	const containerWidth = window.innerWidth;

	function handleMouseMove(e) {
		if (!$.get(isResizing)) return;

		const deltaX = e.clientX - startX;
		const deltaPercent = deltaX / containerWidth * 100;
		const newWidth = Math.max(15, Math.min(60, startWidth + deltaPercent));

		$.set(codexWidth, newWidth);
	}

	function handleMouseUp() {
		$.set(isResizing, false);
		document.removeEventListener("mousemove", handleMouseMove);
		document.removeEventListener("mouseup", handleMouseUp);
	}

	document.addEventListener("mousemove", handleMouseMove);
	document.addEventListener("mouseup", handleMouseUp);
}

var root_1 = $.add_locations($.from_html(`<div class="loading-overlay s-o4RodXlhkyum"><div class="loading-spinner s-o4RodXlhkyum"></div> <p class="loading-text s-o4RodXlhkyum"><!></p></div>`), LearnTab[$.FILENAME], [[93, 6, [[94, 8], [95, 8]]]]);
var root_6 = $.add_locations($.from_html(`<button aria-label="Resize codex panel"></button>`), LearnTab[$.FILENAME], [[122, 6]]);
var root_7 = $.add_locations($.from_html(`<div><!></div>`), LearnTab[$.FILENAME], [[133, 8]]);
var root_9 = $.add_locations($.from_html(`<div><!></div>`), LearnTab[$.FILENAME], [[143, 8]]);
var root_11 = $.add_locations($.from_html(`<div><!></div>`), LearnTab[$.FILENAME], [[152, 8]]);
var root = $.add_locations($.from_html(`<div class="learn-tab s-o4RodXlhkyum"><div><!> <div><!></div> <!> <div><!></div></div></div>`), LearnTab[$.FILENAME], [[88, 0, [[90, 2, [[108, 4], [131, 4]]]]]]);

function LearnTab($$anchor, $$props) {
	$.check_target(new.target);
	$.push($$props, false, LearnTab);

	let currentView = $.mutable_source(LearnView.LESSON_SELECTOR);
	let selectedMode = $.mutable_source(QuizMode.FIXED_QUESTION);
	let currentLessonType = $.mutable_source(null);
	let currentResults = $.mutable_source(null);
	let isLoading = $.mutable_source(false);
	let codexVisible = $.mutable_source(true);
	let codexWidth = $.mutable_source(50);
	let isResizing = $.mutable_source(false);
	let availableLessons = Object.values(LessonType);

	function handleLessonRequested(event) {
		$.set(isLoading, true);

		setTimeout(
			() => {
				$.set(currentLessonType, event.lessonType);
				$.set(selectedMode, event.quizMode);
				$.set(currentView, LearnView.LESSON_WORKSPACE);
				$.set(currentResults, null);
				$.set(isLoading, false);
			},
			300
		);
	}

	function handleModeChanged(mode) {
		$.set(selectedMode, mode);
	}

	function handleBackToSelector() {
		$.set(isLoading, true);

		setTimeout(
			() => {
				$.set(currentView, LearnView.LESSON_SELECTOR);
				$.set(currentLessonType, null);
				$.set(currentResults, null);
				$.set(isLoading, false);
			},
			200
		);
	}

	function handleRetryLesson() {
		if ($.get(currentLessonType)) {
			$.set(isLoading, true);

			setTimeout(
				() => {
					$.set(currentView, LearnView.LESSON_WORKSPACE);
					$.set(currentResults, null);
					$.set(isLoading, false);
				},
				200
			);
		}
	}

	function handleCodexToggle() {
		$.set(codexVisible, !$.get(codexVisible));
	}

	function handlePictographSelected(pictograph) {}

	function handleLessonComplete(results) {
		$.set(isLoading, true);

		setTimeout(
			() => {
				$.set(currentResults, results);
				$.set(currentView, LearnView.LESSON_RESULTS);
				$.set(isLoading, false);
			},
			500
		);
	}

	$.init();

	var div = root();
	var div_1 = $.child(div);
	let classes;
	var node = $.child(div_1);

	{
		var consequent_2 = ($$anchor) => {
			var div_2 = root_1();
			var p = $.sibling($.child(div_2), 2);
			var node_1 = $.child(p);

			{
				var consequent = ($$anchor) => {
					var text = $.text('Loading lessons...');

					$.append($$anchor, text);
				};

				var alternate_1 = ($$anchor) => {
					var fragment = $.comment();
					var node_2 = $.first_child(fragment);

					{
						var consequent_1 = ($$anchor) => {
							var text_1 = $.text('Starting lesson...');

							$.append($$anchor, text_1);
						};

						var alternate = ($$anchor) => {
							var text_2 = $.text('Processing results...');

							$.append($$anchor, text_2);
						};

						$.add_svelte_meta(
							() => $.if(
								node_2,
								($$render) => {
									if ($.strict_equals($.get(currentView), LearnView.LESSON_WORKSPACE)) $$render(consequent_1); else $$render(alternate, false);
								},
								true
							),
							'if',
							LearnTab,
							98,
							10
						);
					}

					$.append($$anchor, fragment);
				};

				$.add_svelte_meta(
					() => $.if(node_1, ($$render) => {
						if ($.strict_equals($.get(currentView), LearnView.LESSON_SELECTOR)) $$render(consequent); else $$render(alternate_1, false);
					}),
					'if',
					LearnTab,
					96,
					10
				);
			}

			$.reset(p);
			$.reset(div_2);
			$.append($$anchor, div_2);
		};

		$.add_svelte_meta(
			() => $.if(node, ($$render) => {
				if ($.get(isLoading)) $$render(consequent_2);
			}),
			'if',
			LearnTab,
			92,
			4
		);
	}

	var div_3 = $.sibling(node, 2);
	let classes_1;
	var node_3 = $.child(div_3);

	$.add_svelte_meta(
		() => CodexComponent(node_3, {
			get isVisible() {
				return $.get(codexVisible);
			},

			onPictographSelected: handlePictographSelected,
			onToggleVisibility: handleCodexToggle
		}),
		'component',
		LearnTab,
		113,
		6,
		{ componentTag: 'CodexComponent' }
	);

	$.reset(div_3);

	var node_4 = $.sibling(div_3, 2);

	{
		var consequent_3 = ($$anchor) => {
			var button = root_6();
			let classes_2;

			button.__mousedown = [handleResizeStart, isResizing, codexWidth];
			$.template_effect(($0) => classes_2 = $.set_class(button, 1, 'resize-handle s-o4RodXlhkyum', null, classes_2, $0), [() => ({ resizing: $.get(isResizing) })]);
			$.append($$anchor, button);
		};

		$.add_svelte_meta(
			() => $.if(node_4, ($$render) => {
				if ($.get(codexVisible)) $$render(consequent_3);
			}),
			'if',
			LearnTab,
			121,
			4
		);
	}

	var div_4 = $.sibling(node_4, 2);
	let classes_3;
	var node_5 = $.child(div_4);

	{
		var consequent_4 = ($$anchor) => {
			var div_5 = root_7();
			let classes_4;
			var node_6 = $.child(div_5);

			$.add_svelte_meta(
				() => LessonSelectorView(node_6, {
					get availableLessons() {
						return availableLessons;
					},

					isLoading: false,
					onLessonRequested: handleLessonRequested,
					onModeChanged: handleModeChanged,

					get selectedMode() {
						return $.get(selectedMode);
					},

					set selectedMode($$value) {
						$.set(selectedMode, $$value);
					},

					$$legacy: true
				}),
				'component',
				LearnTab,
				134,
				10,
				{ componentTag: 'LessonSelectorView' }
			);

			$.reset(div_5);
			$.template_effect(($0) => classes_4 = $.set_class(div_5, 1, 'view-container s-o4RodXlhkyum', null, classes_4, $0), [() => ({ active: !$.get(isLoading) })]);
			$.append($$anchor, div_5);
		};

		var alternate_3 = ($$anchor) => {
			var fragment_1 = $.comment();
			var node_7 = $.first_child(fragment_1);

			{
				var consequent_5 = ($$anchor) => {
					var div_6 = root_9();
					let classes_5;
					var node_8 = $.child(div_6);

					$.add_svelte_meta(
						() => LessonWorkspaceView(node_8, {
							get lessonType() {
								return $.get(currentLessonType);
							},

							get quizMode() {
								return $.get(selectedMode);
							},

							onBackToSelector: handleBackToSelector,
							onLessonComplete: handleLessonComplete
						}),
						'component',
						LearnTab,
						144,
						10,
						{ componentTag: 'LessonWorkspaceView' }
					);

					$.reset(div_6);
					$.template_effect(($0) => classes_5 = $.set_class(div_6, 1, 'view-container s-o4RodXlhkyum', null, classes_5, $0), [() => ({ active: !$.get(isLoading) })]);
					$.append($$anchor, div_6);
				};

				var alternate_2 = ($$anchor) => {
					var fragment_2 = $.comment();
					var node_9 = $.first_child(fragment_2);

					{
						var consequent_6 = ($$anchor) => {
							var div_7 = root_11();
							let classes_6;
							var node_10 = $.child(div_7);

							$.add_svelte_meta(
								() => LessonResultsView(node_10, {
									get results() {
										return $.get(currentResults);
									},

									onBackToSelector: handleBackToSelector,
									onRetryLesson: handleRetryLesson
								}),
								'component',
								LearnTab,
								153,
								10,
								{ componentTag: 'LessonResultsView' }
							);

							$.reset(div_7);
							$.template_effect(($0) => classes_6 = $.set_class(div_7, 1, 'view-container s-o4RodXlhkyum', null, classes_6, $0), [() => ({ active: !$.get(isLoading) })]);
							$.append($$anchor, div_7);
						};

						$.add_svelte_meta(
							() => $.if(
								node_9,
								($$render) => {
									if ($.strict_equals($.get(currentView), LearnView.LESSON_RESULTS)) $$render(consequent_6);
								},
								true
							),
							'if',
							LearnTab,
							151,
							6
						);
					}

					$.append($$anchor, fragment_2);
				};

				$.add_svelte_meta(
					() => $.if(
						node_7,
						($$render) => {
							if ($.strict_equals($.get(currentView), LearnView.LESSON_WORKSPACE)) $$render(consequent_5); else $$render(alternate_2, false);
						},
						true
					),
					'if',
					LearnTab,
					142,
					6
				);
			}

			$.append($$anchor, fragment_1);
		};

		$.add_svelte_meta(
			() => $.if(node_5, ($$render) => {
				if ($.strict_equals($.get(currentView), LearnView.LESSON_SELECTOR)) $$render(consequent_4); else $$render(alternate_3, false);
			}),
			'if',
			LearnTab,
			132,
			6
		);
	}

	$.reset(div_4);
	$.reset(div_1);
	$.reset(div);

	$.template_effect(
		($0, $1, $2) => {
			classes = $.set_class(div_1, 1, 'learn-content-wrapper s-o4RodXlhkyum', null, classes, $0);
			classes_1 = $.set_class(div_3, 1, 'codex-sidebar s-o4RodXlhkyum', null, classes_1, $1);
			$.set_style(div_3, `width: ${$.get(codexVisible) ? $.get(codexWidth) + '%' : '48px'}`);
			classes_3 = $.set_class(div_4, 1, 'learn-content s-o4RodXlhkyum', null, classes_3, $2);
		},
		[
			() => ({ 'codex-visible': $.get(codexVisible) }),
			() => ({ collapsed: !$.get(codexVisible) }),
			() => ({ loading: $.get(isLoading) })
		]
	);

	$.append($$anchor, div);

	return $.pop({ ...$.legacy_api() });
}

if (import.meta.hot) {
	LearnTab = $.hmr(LearnTab, () => LearnTab[$.HMR].source);

	import.meta.hot.acceptExports(["default"],(module) => {
		$.cleanup_styles('s-o4RodXlhkyum');
