function handlePictographChange(event, $$props) {
	const target = event.target;
	const pictograph = $$props.availablePictographs.find((p) => $.strict_equals(p.id, target.value));

	if (pictograph) {
		$$props.onPictographSelect(pictograph);
	}
}

function handleArrowColorChange(event, $$props) {
	const target = event.target;

	$$props.onArrowColorSelect(target.value);
}

