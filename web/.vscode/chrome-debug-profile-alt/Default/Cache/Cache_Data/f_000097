function ExportSettingsCard($$anchor, $$props) {
	$.check_target(new.target);
	$.push($$props, true, ExportSettingsCard);

	const userOptions = [
		{ value: "Default User", label: "Default User" },
		{ value: "Custom", label: "Custom..." }
	];

	function handleCheckboxChange(setting, checked) {
		$$props.onsettingchanged?.({ setting, value: checked });
	}

	function handleSelectChange(setting, value) {
		$$props.onsettingchanged?.({ setting, value });
	}

	function handleTextChange(setting, value) {
		$$props.onsettingchanged?.({ setting, value });
	}

	var div = root();
	var div_1 = $.sibling($.child(div), 2);
	var div_2 = $.sibling($.child(div_1), 2);
	var label = $.child(div_2);
	var input = $.child(label);

	$.remove_input_defaults(input);
	input.__change = [on_change, handleCheckboxChange];
	$.next(4);
	$.reset(label);

	var label_1 = $.sibling(label, 2);
	var input_1 = $.child(label_1);

	$.remove_input_defaults(input_1);
	input_1.__change = [on_change_1, handleCheckboxChange];
	$.next(4);
	$.reset(label_1);

	var label_2 = $.sibling(label_1, 2);
	var input_2 = $.child(label_2);

	$.remove_input_defaults(input_2);
	input_2.__change = [on_change_2, handleCheckboxChange];
	$.next(4);
	$.reset(label_2);

	var label_3 = $.sibling(label_2, 2);
	var input_3 = $.child(label_3);

	$.remove_input_defaults(input_3);
	input_3.__change = [on_change_3, handleCheckboxChange];
	$.next(4);
	$.reset(label_3);

	var label_4 = $.sibling(label_3, 2);
	var input_4 = $.child(label_4);

	$.remove_input_defaults(input_4);
	input_4.__change = [on_change_4, handleCheckboxChange];
	$.next(4);
	$.reset(label_4);

	var label_5 = $.sibling(label_4, 2);
	var input_5 = $.child(label_5);

	$.remove_input_defaults(input_5);
	input_5.__change = [on_change_5, handleCheckboxChange];
	$.next(4);
	$.reset(label_5);
	$.reset(div_2);
	$.reset(div_1);

	var div_3 = $.sibling(div_1, 6);
	var div_4 = $.child(div_3);
	var select = $.sibling($.child(div_4), 2);

	select.__change = [on_change_6, handleSelectChange];

	$.add_svelte_meta(
		() => $.each(select, 21, () => userOptions, $.index, ($$anchor, option) => {
			var option_1 = root_1();
			var text = $.child(option_1, true);

			$.reset(option_1);

			var option_1_value = {};

			$.template_effect(() => {
				$.set_text(text, $.get(option).label);

				if (option_1_value !== (option_1_value = $.get(option).value)) {
					option_1.value = (option_1.__value = $.get(option).value) ?? '';
				}
			});

			$.append($$anchor, option_1);
		}),
		'each',
		ExportSettingsCard,
		134,
		8
	);

	$.reset(select);

	var select_value;

	$.init_select(select);
	$.reset(div_4);

	var div_5 = $.sibling(div_4, 2);
	var textarea = $.sibling($.child(div_5), 2);

	$.remove_textarea_child(textarea);
	textarea.__input = [on_input, handleTextChange];
	$.reset(div_5);
	$.reset(div_3);
	$.reset(div);

	$.template_effect(() => {
		$.set_checked(input, $$props.exportSettings.include_start_position);
		$.set_checked(input_1, $$props.exportSettings.add_beatNumbers);
		$.set_checked(input_2, $$props.exportSettings.add_reversal_symbols);
		$.set_checked(input_3, $$props.exportSettings.add_user_info);
		$.set_checked(input_4, $$props.exportSettings.add_word);
		$.set_checked(input_5, $$props.exportSettings.use_last_save_directory);

		if (select_value !== (select_value = $$props.exportSettings.user_name)) {
			(
				select.value = (select.__value = $$props.exportSettings.user_name) ?? '',
				$.select_option(select, $$props.exportSettings.user_name)
			);
		}

		$.set_value(textarea, $$props.exportSettings.custom_note);
	});

	$.append($$anchor, div);

	return $.pop({ ...$.legacy_api() });
}

if (import.meta.hot) {
	ExportSettingsCard = $.hmr(ExportSettingsCard, () => ExportSettingsCard[$.HMR].source);

	import.meta.hot.acceptExports(["default"],(module) => {
		$.cleanup_styles('s-BK6SWuzcuUKY');
		module.default[$.HMR].source = ExportSettingsCard[$.HMR].source;
		$.set(ExportSettingsCard[$.HMR].source, module.default[$.HMR].original);
	});
}

export default ExportSettingsCard;

$.delegate(['change', 'input']);
