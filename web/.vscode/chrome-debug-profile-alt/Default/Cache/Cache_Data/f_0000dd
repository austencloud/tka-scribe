:root {
  /* Primary Colors - matching desktop PyQt6 styling */
  --desktop-primary-blue: rgba(70, 130, 180, 0.8);
  --desktop-primary-blue-hover: rgba(100, 149, 237, 0.9);
  --desktop-primary-blue-active: rgba(65, 105, 225, 1);
