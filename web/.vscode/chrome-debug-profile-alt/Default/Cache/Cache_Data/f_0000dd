/* Import Glass Scrollbars System */

/**
 * Desktop Glassmorphism Scrollbars
 *
 * A comprehensive scrollbar styling system that matches the desktop theme
 * with beautiful glassmorphism effects. Provides consistent, elegant
 * scrollbars across all components.
 *
 * Usage: Add the appropriate class to any scrollable container:
 * - .glass-scrollbar-primary (main scrollbars)
 * - .glass-scrollbar-secondary (subtle scrollbars)
 * - .glass-scrollbar-minimal (ultra-thin scrollbars)
 * - .glass-scrollbar-hover (appears only on hover)
 */

/* ===== QUICK GLASS SCROLLBARS (Easy Application) ===== */

/* Simple classes that can be applied to any scrollable element */

.quick-glass-primary {
  overflow-y: auto;
  overflow-x: hidden;
}

.quick-glass-primary::-webkit-scrollbar {
  width: 12px;
}

.quick-glass-primary::-webkit-scrollbar-track {
  background: rgba(255, 255, 255, 0.03);
  border: 1px solid rgba(255, 255, 255, 0.1);
  border-radius: 6px;
  backdrop-filter: blur(10px);
  -webkit-backdrop-filter: blur(10px);
}

.quick-glass-primary::-webkit-scrollbar-thumb {
  background: rgba(255, 255, 255, 0.15);
  border: 1px solid rgba(255, 255, 255, 0.2);
  border-radius: 6px;
  backdrop-filter: blur(15px);
  -webkit-backdrop-filter: blur(15px);
  transition: all 0.3s ease;
}

.quick-glass-primary::-webkit-scrollbar-thumb:hover {
  background: rgba(255, 255, 255, 0.25);
  border-color: rgba(255, 255, 255, 0.3);
  transform: scale(1.05);
}

.quick-glass-secondary {
  overflow-y: auto;
  overflow-x: hidden;
}

.quick-glass-secondary::-webkit-scrollbar {
  width: 10px;
}

.quick-glass-secondary::-webkit-scrollbar-track {
  background: rgba(255, 255, 255, 0.02);
  border: 1px solid rgba(255, 255, 255, 0.05);
  border-radius: 6px;
  backdrop-filter: blur(8px);
  -webkit-backdrop-filter: blur(8px);
}

.quick-glass-secondary::-webkit-scrollbar-thumb {
  background: rgba(255, 255, 255, 0.1);
  border: 1px solid rgba(255, 255, 255, 0.15);
  border-radius: 6px;
  backdrop-filter: blur(12px);
  -webkit-backdrop-filter: blur(12px);
  transition: all 0.3s ease;
}

.quick-glass-secondary::-webkit-scrollbar-thumb:hover {
  background: rgba(255, 255, 255, 0.18);
  border-color: rgba(255, 255, 255, 0.25);
}

.quick-glass-minimal {
  overflow-y: auto;
  overflow-x: hidden;
}

.quick-glass-minimal::-webkit-scrollbar {
  width: 6px;
}

.quick-glass-minimal::-webkit-scrollbar-track {
  background: transparent;
}

.quick-glass-minimal::-webkit-scrollbar-thumb {
  background: rgba(255, 255, 255, 0.12);
  border-radius: 3px;
  backdrop-filter: blur(8px);
  -webkit-backdrop-filter: blur(8px);
  transition: all 0.3s ease;
}

.quick-glass-minimal::-webkit-scrollbar-thumb:hover {
  background: rgba(255, 255, 255, 0.2);
}

.quick-glass-hover {
  overflow-y: auto;
  overflow-x: hidden;
}

.quick-glass-hover::-webkit-scrollbar {
  width: 0px;
  transition: width 0.3s ease;
}

.quick-glass-hover:hover::-webkit-scrollbar {
  width: 12px;
}

.quick-glass-hover::-webkit-scrollbar-track {
  background: rgba(255, 255, 255, 0.03);
  border: 1px solid rgba(255, 255, 255, 0.1);
  border-radius: 6px;
  backdrop-filter: blur(10px);
  -webkit-backdrop-filter: blur(10px);
  opacity: 0;
  transition: opacity 0.3s ease;
}

.quick-glass-hover:hover::-webkit-scrollbar-track {
  opacity: 1;
}

.quick-glass-hover::-webkit-scrollbar-thumb {
  background: rgba(255, 255, 255, 0.15);
  border: 1px solid rgba(255, 255, 255, 0.2);
  border-radius: 6px;
  backdrop-filter: blur(15px);
  -webkit-backdrop-filter: blur(15px);
  opacity: 0;
  transition: all 0.3s ease;
}

.quick-glass-hover:hover::-webkit-scrollbar-thumb {
  opacity: 1;
}

.quick-glass-gradient {
  overflow-y: auto;
  overflow-x: hidden;
}

.quick-glass-gradient::-webkit-scrollbar {
  width: 12px;
}

.quick-glass-gradient::-webkit-scrollbar-track {
  background: linear-gradient(
    180deg,
    rgba(255, 255, 255, 0.05) 0%,
    rgba(255, 255, 255, 0.02) 50%,
    rgba(255, 255, 255, 0.05) 100%
  );
  border: 1px solid rgba(255, 255, 255, 0.1);
  border-radius: 6px;
