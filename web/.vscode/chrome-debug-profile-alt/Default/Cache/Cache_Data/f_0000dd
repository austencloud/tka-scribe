  --desktop-restart-blue-hover: rgba(62, 99, 221, 0.8);
  --desktop-restart-blue-active: rgba(62, 99, 221, 0.9);
  --desktop-restart-blue-border: rgba(62, 99, 221, 0.9);
  --desktop-restart-blue-hover-border: rgba(82, 119, 241, 1);

  /* Background and Surface Colors */
  --desktop-bg-primary: rgba(255, 255, 255, 0.2);
  --desktop-bg-secondary: rgba(255, 255, 255, 0.1);
  --desktop-bg-tertiary: rgba(255, 255, 255, 0.05);
  --desktop-border-primary: rgba(255, 255, 255, 0.3);
  --desktop-border-secondary: rgba(255, 255, 255, 0.2);
  --desktop-border-tertiary: rgba(255, 255, 255, 0.1);

  /* Progress Colors */
  --desktop-progress-excellent: rgba(0, 255, 0, 0.9);
  --desktop-progress-good: rgba(255, 255, 0, 0.9);
  --desktop-progress-fair: rgba(255, 165, 0, 0.9);
  --desktop-progress-poor: rgba(255, 0, 0, 0.9);

  /* Text Colors */
  --desktop-text-primary: rgba(255, 255, 255, 1);
  --desktop-text-secondary: rgba(255, 255, 255, 0.9);
  --desktop-text-muted: rgba(255, 255, 255, 0.7);
  --desktop-text-disabled: rgba(255, 255, 255, 0.5);

  /* Typography - Georgia serif to match desktop */
