				if ($$props.beat.pictographData) $$render(consequent); else $$render(alternate, false);
			}),
			'if',
			Beat,
			61,
			2
		);
	}

	var div_3 = $.sibling(node, 2);
	var node_2 = $.child(div_3);

	{
