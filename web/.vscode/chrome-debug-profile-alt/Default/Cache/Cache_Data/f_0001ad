			$.append($$anchor, div_5);
		};

		$.add_svelte_meta(
			() => $.if(node_3, ($$render) => {
				if ($.get(isStartPosition)()) $$render(consequent_2);
			}),
			'if',
			Beat,
			82,
			4
		);
	}

	var node_4 = $.sibling(node_3, 2);

	{
		var consequent_5 = ($$anchor) => {
			var div_6 = root_5();
