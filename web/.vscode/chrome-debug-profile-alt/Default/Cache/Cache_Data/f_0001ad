		width = $.prop($$props, 'width', 3, 200),
		height = $.prop($$props, 'height', 3, 200);

	const isStartPosition = $.tag(
		$.derived(() => () => {
			return $.strict_equals($$props.beat.beatNumber, 0) || $.strict_equals($$props.beat.metadata?.isStartPosition, true) || $.strict_equals($$props.beat.metadata?.type, "start");
		}),
		'isStartPosition'
	);

	const displayBeatNumber = $.tag(
