		}),
		'ariaLabel'
	);

	function handleKeyPress(event) {
		if ($.strict_equals(event.key, "Enter") || $.strict_equals(event.key, " ")) {
			event.preventDefault();
			$$props.onClick?.();
		}
	}

	var div = root();
	let classes;

	div.__click = [handleClick, $$props];

	let styles;
	var node = $.child(div);

	{
		var consequent = ($$anchor) => {
			var fragment = $.comment();
			var node_1 = $.first_child(fragment);

			$.add_svelte_meta(
				() => Pictograph(node_1, {
					get pictographData() {
						return $$props.beat.pictographData;
