import { createHotContext as __vite__createHotContext } from "/@vite/client";import.meta.hot = __vite__createHotContext("/src/lib/components/settings/tabs/GeneralTab.svelte");import "/node_modules/.vite/deps/svelte_internal_disclose-version.js?v=608ae08a";

GeneralTab[$.FILENAME] = 'src/lib/components/settings/tabs/GeneralTab.svelte';

import * as $ from "/node_modules/.vite/deps/svelte_internal_client.js?v=608ae08a";
import { GridMode } from "/src/lib/domain/index.ts";
import SelectInput from "/src/lib/components/settings/SelectInput.svelte";
import SettingCard from "/src/lib/components/settings/SettingCard.svelte";
import TextInput from "/src/lib/components/settings/TextInput.svelte";
import ToggleSetting from "/src/lib/components/settings/ToggleSetting.svelte";

var root_3 = $.add_locations($.from_html(`<!> <!> <!> <!>`, 1), GeneralTab[$.FILENAME], []);
var root = $.add_locations($.from_html(`<div class="tab-content s-Tg6FjmA7jaGB"><!> <!> <!></div>`), GeneralTab[$.FILENAME], [[67, 0]]);

function GeneralTab($$anchor, $$props) {
	$.check_target(new.target);
	$.push($$props, true, GeneralTab);
	console.log(...$.log_if_contains_state('log', "\u{1F50D} GeneralTab initialized, onupdate function:", typeof $$props.onupdate, $$props.onupdate));

	let userName = $.tag($.state($.proxy($$props.settings.userName || "")), 'userName');
	let autoSave = $.tag($.state($.proxy($$props.settings.autoSave ?? true)), 'autoSave');
	let gridMode = $.tag($.state($.proxy($$props.settings.gridMode || GridMode.DIAMOND)), 'gridMode');
	let workbenchColumns = $.tag($.state($.proxy($$props.settings.workbenchColumns || 5)), 'workbenchColumns');
	let animationsEnabled = $.tag($.state($.proxy($$props.settings.animationsEnabled ?? true)), 'animationsEnabled');
	let developerMode = $.tag($.state($.proxy($$props.settings.developerMode ?? false)), 'developerMode');

	const gridModeOptions = [
		{ value: GridMode.DIAMOND, label: "Diamond" },
		{ value: GridMode.BOX, label: "Box" }
	];

	function handleAnimationsEnabledChange(checked) {
		$.set(animationsEnabled, checked, true);
		$$props.onupdate?.({ key: "animationsEnabled", value: $.get(animationsEnabled) });
		console.log(`\u{1F3A8} Animations ${$.get(animationsEnabled) ? "enabled" : "disabled"}`);
	}

	function handleDeveloperModeChange(checked) {
		console.log(...$.log_if_contains_state('log', "\u{1F527} Developer mode toggle clicked, new value:", checked));
		console.log("\u{1F527} onupdate function available:", typeof $$props.onupdate);
		$.set(developerMode, checked, true);

		import("/src/lib/state/app-state.svelte.ts").then(({ updateSettings, getSettings }) => {
			const currentSettings = getSettings();
			const newSettings = { ...currentSettings, developerMode: checked };

			console.log(...$.log_if_contains_state('log', "\u{1F527} Direct updateSettings call with:", newSettings));
			updateSettings(newSettings);
		});

		const updateData = { key: "developerMode", value: $.get(developerMode) };

		console.log(...$.log_if_contains_state('log', "\u{1F527} About to call onupdate with:", updateData));
		$$props.onupdate?.(updateData);
		console.log(`\u{1F527} Developer mode ${$.get(developerMode) ? "enabled" : "disabled"} - onupdate called`);
	}

	function handleUserNameChange(value) {
		$.set(userName, value, true);
		$$props.onupdate?.({ key: "userName", value: $.get(userName) });
	}

	function handleAutoSaveChange(checked) {
		$.set(autoSave, checked, true);
		$$props.onupdate?.({ key: "autoSave", value: $.get(autoSave) });
	}

	function handleGridModeChange(value) {
		if ($.strict_equals(value, GridMode.DIAMOND) || $.strict_equals(value, GridMode.BOX)) {
			$.set(gridMode, value, true);
			$$props.onupdate?.({ key: "gridMode", value: $.get(gridMode) });
		}
	}

	function handleWorkbenchColumnsChange(value) {
		$.set(workbenchColumns, parseInt(value), true);
		$$props.onupdate?.({ key: "workbenchColumns", value: $.get(workbenchColumns) });
	}

	var div = root();
	var node = $.child(div);

	$.add_svelte_meta(
		() => SettingCard(node, {
			title: 'User Profile',

			children: $.wrap_snippet(GeneralTab, ($$anchor, $$slotProps) => {
				var fragment = $.comment();
				var node_1 = $.first_child(fragment);

				$.add_svelte_meta(
					() => TextInput(node_1, {
						label: 'User Name',

						get value() {
							return $.get(userName);
						},

						placeholder: 'Enter your name...',
						maxlength: 50,
						helpText: 'Appears on exported sequences',
						onchange: handleUserNameChange
					}),
					'component',
					GeneralTab,
					69,
					4,
					{ componentTag: 'TextInput' }
				);

				$.append($$anchor, fragment);
			}),

			$$slots: { default: true }
		}),
		'component',
		GeneralTab,
		68,
		2,
		{ componentTag: 'SettingCard' }
	);

	var node_2 = $.sibling(node, 2);

	$.add_svelte_meta(
		() => SettingCard(node_2, {
			title: 'Animation Settings',

			children: $.wrap_snippet(GeneralTab, ($$anchor, $$slotProps) => {
				var fragment_1 = $.comment();
				var node_3 = $.first_child(fragment_1);

				$.add_svelte_meta(
					() => ToggleSetting(node_3, {
						label: 'Enable Animations',

						get checked() {
							return $.get(animationsEnabled);
						},

						helpText: 'Enable smooth animations and transitions',
						onchange: handleAnimationsEnabledChange
					}),
					'component',
					GeneralTab,
					80,
					4,
					{ componentTag: 'ToggleSetting' }
				);

				$.append($$anchor, fragment_1);
			}),

			$$slots: { default: true }
		}),
		'component',
		GeneralTab,
		79,
		2,
		{ componentTag: 'SettingCard' }
	);

	var node_4 = $.sibling(node_2, 2);

	$.add_svelte_meta(
		() => SettingCard(node_4, {
			title: 'Application Settings',

			children: $.wrap_snippet(GeneralTab, ($$anchor, $$slotProps) => {
				var fragment_2 = root_3();
				var node_5 = $.first_child(fragment_2);

				$.add_svelte_meta(
					() => ToggleSetting(node_5, {
						label: 'Auto-save Settings',

						get checked() {
							return $.get(autoSave);
						},

						helpText: 'Save changes automatically',
						onchange: handleAutoSaveChange
					}),
					'component',
					GeneralTab,
					89,
					4,
					{ componentTag: 'ToggleSetting' }
				);

				var node_6 = $.sibling(node_5, 2);

				$.add_svelte_meta(
					() => ToggleSetting(node_6, {
						label: 'Developer Mode',

						get checked() {
							return $.get(developerMode);
						},

						helpText: 'Show developer tools and experimental features in navigation',
						onchange: handleDeveloperModeChange
					}),
					'component',
					GeneralTab,
					96,
					4,
					{ componentTag: 'ToggleSetting' }
				);

				var node_7 = $.sibling(node_6, 2);

				$.add_svelte_meta(
					() => SelectInput(node_7, {
						label: 'Grid Mode',

						get value() {
							return $.get(gridMode);
						},

						get options() {
							return gridModeOptions;
						},

						helpText: 'Pictograph grid layout style',
						onchange: handleGridModeChange
					}),
					'component',
					GeneralTab,
					103,
					4,
					{ componentTag: 'SelectInput' }
				);

				var node_8 = $.sibling(node_7, 2);

				{
					let $0 = $.derived(() => $.get(workbenchColumns).toString());

					$.add_svelte_meta(
						() => TextInput(node_8, {
							label: 'Workbench Columns',

							get value() {
								return $.get($0);
							},

							type: 'number',
							min: 1,
							max: 12,
							helpText: 'Number of columns in sequence workbench',
							onchange: handleWorkbenchColumnsChange
						}),
						'component',
						GeneralTab,
						111,
						4,
						{ componentTag: 'TextInput' }
					);
				}

				$.append($$anchor, fragment_2);
			}),

			$$slots: { default: true }
		}),
		'component',
		GeneralTab,
		88,
		2,
		{ componentTag: 'SettingCard' }
	);

	$.reset(div);
	$.append($$anchor, div);

	return $.pop({ ...$.legacy_api() });
}

if (import.meta.hot) {
	GeneralTab = $.hmr(GeneralTab, () => GeneralTab[$.HMR].source);

	import.meta.hot.acceptExports(["default"],(module) => {
		$.cleanup_styles('s-Tg6FjmA7jaGB');
		module.default[$.HMR].source = GeneralTab[$.HMR].source;
		$.set(GeneralTab[$.HMR].source, module.default[$.HMR].original);
	});
}

export default GeneralTab;
import "/src/lib/components/settings/tabs/GeneralTab.svelte?svelte&type=style&lang.css";

//# sourceMappingURL=data:application/json;base64,