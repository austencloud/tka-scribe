import { createHotContext as __vite__createHotContext } from "/@vite/client";import.meta.hot = __vite__createHotContext("/src/lib/components/construct/start-position/StartPositionPicker.svelte");import "/node_modules/.vite/deps/svelte_internal_disclose-version.js?v=608ae08a";

StartPositionPicker[$.FILENAME] = 'src/lib/components/construct/start-position/StartPositionPicker.svelte';

import * as $ from "/node_modules/.vite/deps/svelte_internal_client.js?v=608ae08a";
import { GridMode } from "/src/lib/domain/index.ts";
import { resolve } from "/src/lib/services/bootstrap.ts";
import ErrorState from "/src/lib/components/construct/start-position/ui/ErrorState.svelte";
import LoadingState from "/src/lib/components/construct/start-position/ui/LoadingState.svelte";
import PictographGrid from "/src/lib/components/construct/start-position/ui/PictographGrid.svelte";
import TransitionOverlay from "/src/lib/components/construct/start-position/ui/TransitionOverlay.svelte";

var root = $.add_locations($.from_html(`<div class="start-pos-picker s-yKJ0n_ggW8mu" data-testid="start-position-picker"><!> <div style="position: absolute; top: 10px; right: 10px; background: rgba(0,0,0,0.8); color: white; padding: 5px; font-size: 10px;" class="s-yKJ0n_ggW8mu"> </div> <!></div>`), StartPositionPicker[$.FILENAME], [[95, 0, [[114, 2]]]]);

function StartPositionPicker($$anchor, $$props) {
	$.check_target(new.target);
	$.push($$props, true, StartPositionPicker);

	const sequenceStateService = resolve("ISequenceStateService");
	const startPositionService = resolve("IStartPositionService");
	let isLoading = $.tag($.state(true), 'isLoading');
	let loadingError = $.tag($.state(null), 'loadingError');
	let startPositions = $.tag($.state($.proxy([])), 'startPositions');
	let selectedStartPos = $.tag($.state(null), 'selectedStartPos');
	let isTransitioning = $.tag($.state(false), 'isTransitioning');

	$.user_effect(() => {
		loadStartPositions();
	});

	async function loadStartPositions() {
		if (!startPositionService) {
			console.error("\u274C StartPositionPicker: No start position service available");
			$.set(loadingError, "Start position service not available");
			$.set(isLoading, false);

			return;
		}

		try {
			$.set(isLoading, true);
			$.set(loadingError, null);

			const positions = (await $.track_reactivity_loss(startPositionService.getDefaultStartPositions(GridMode.DIAMOND)))();

			$.set(startPositions, positions, true);
		} catch(error) {
			console.error(...$.log_if_contains_state('error', "\u274C StartPositionPicker: Failed to load start positions", error));
			$.set(loadingError, error instanceof Error ? error.message : "Failed to load start positions", true);
			$.set(startPositions, [], true);
		} finally {
			$.set(isLoading, false);
		}
	}

	async function handleSelect(position) {
		$.set(selectedStartPos, position, true);

		try {
			let currentSequence = $$props.sequenceState.currentSequence;

			if (!currentSequence) {
				currentSequence = sequenceStateService.createNewSequence("New Sequence", 16);
				$$props.sequenceState.setCurrentSequence(currentSequence);
			}

			const startPositionBeat = {
				id: crypto.randomUUID(),
				beatNumber: 0,

				// Start position is beat 0
				duration: 1,

				blueReversal: false,
				redReversal: false,
				isBlank: false,
				pictographData: position,

				metadata: {
					isStartPosition: true,
					endPosition: position.letter || "unknown"
				}
			};

			const updatedSequence = sequenceStateService.setStartPosition(currentSequence, startPositionBeat);

			$$props.sequenceState.setCurrentSequence(updatedSequence);
		} catch(error) {
			console.error(...$.log_if_contains_state('error', "\u274C StartPositionPicker: Failed to set start position", error));
		}
	}

	$.user_effect(() => {
		const currentSequence = $$props.sequenceState.currentSequence;

		if (currentSequence && currentSequence.startPosition && $.get(isTransitioning)) {
			$.set(isTransitioning, false);
		}
	});

	var div = root();
	var node = $.child(div);

	{
		var consequent = ($$anchor) => {
			var fragment = $.comment();
			var node_1 = $.first_child(fragment);

			$.add_svelte_meta(() => LoadingState(node_1, {}), 'component', StartPositionPicker, 97, 4, { componentTag: 'LoadingState' });
			$.append($$anchor, fragment);
		};

		var alternate_2 = ($$anchor) => {
			var fragment_1 = $.comment();
			var node_2 = $.first_child(fragment_1);

			{
				var consequent_1 = ($$anchor) => {
					var fragment_2 = $.comment();
					var node_3 = $.first_child(fragment_2);

					$.add_svelte_meta(() => ErrorState(node_3, {}), 'component', StartPositionPicker, 99, 4, { componentTag: 'ErrorState' });
					$.append($$anchor, fragment_2);
				};

				var alternate_1 = ($$anchor) => {
					var fragment_3 = $.comment();
					var node_4 = $.first_child(fragment_3);

					{
						var consequent_2 = ($$anchor) => {
							var fragment_4 = $.comment();
							var node_5 = $.first_child(fragment_4);

							$.add_svelte_meta(
								() => ErrorState(node_5, {
									message: 'No valid start positions found for the current configuration.',
									hasRefreshButton: false
								}),
								'component',
								StartPositionPicker,
								101,
								4,
								{ componentTag: 'ErrorState' }
							);

							$.append($$anchor, fragment_4);
						};

						var alternate = ($$anchor) => {
							var fragment_5 = $.comment();
							var node_6 = $.first_child(fragment_5);

							$.add_svelte_meta(
								() => PictographGrid(node_6, {
									get pictographDataSet() {
										return $.get(startPositions);
									},

									get selectedPictograph() {
										return $.get(selectedStartPos);
									},

									onPictographSelect: handleSelect
								}),
								'component',
								StartPositionPicker,
								106,
								4,
								{ componentTag: 'PictographGrid' }
							);

							$.append($$anchor, fragment_5);
						};

						$.add_svelte_meta(
							() => $.if(
								node_4,
								($$render) => {
									if ($.strict_equals($.get(startPositions).length, 0)) $$render(consequent_2); else $$render(alternate, false);
								},
								true
							),
							'if',
							StartPositionPicker,
							100,
							2
						);
					}

					$.append($$anchor, fragment_3);
				};

				$.add_svelte_meta(
					() => $.if(
						node_2,
						($$render) => {
							if ($.get(loadingError)) $$render(consequent_1); else $$render(alternate_1, false);
						},
						true
					),
					'if',
					StartPositionPicker,
					98,
					2
				);
			}

			$.append($$anchor, fragment_1);
		};

		$.add_svelte_meta(
			() => $.if(node, ($$render) => {
				if ($.get(isLoading)) $$render(consequent); else $$render(alternate_2, false);
			}),
			'if',
			StartPositionPicker,
			96,
			2
		);
	}

	var div_1 = $.sibling(node, 2);
	var text = $.child(div_1);

	$.reset(div_1);

	var node_7 = $.sibling(div_1, 2);

	{
		var consequent_3 = ($$anchor) => {
			var fragment_6 = $.comment();
			var node_8 = $.first_child(fragment_6);

			$.add_svelte_meta(() => TransitionOverlay(node_8, {}), 'component', StartPositionPicker, 122, 4, { componentTag: 'TransitionOverlay' });
			$.append($$anchor, fragment_6);
		};

		$.add_svelte_meta(
			() => $.if(node_7, ($$render) => {
				if ($.get(isTransitioning)) $$render(consequent_3);
			}),
			'if',
			StartPositionPicker,
			121,
			2
		);
	}

	$.reset(div);
	$.template_effect(() => $.set_text(text, `Debug: isLoading=${$.get(isLoading) ?? ''}, pictographs=${$.get(startPositions).length ?? ''}, error=${$.get(loadingError) ?? ''}`));
	$.append($$anchor, div);

	return $.pop({ ...$.legacy_api() });
}

if (import.meta.hot) {
	StartPositionPicker = $.hmr(StartPositionPicker, () => StartPositionPicker[$.HMR].source);

	import.meta.hot.acceptExports(["default"],(module) => {
		$.cleanup_styles('s-yKJ0n_ggW8mu');
		module.default[$.HMR].source = StartPositionPicker[$.HMR].source;
		$.set(StartPositionPicker[$.HMR].source, module.default[$.HMR].original);
	});
}

export default StartPositionPicker;
import "/src/lib/components/construct/start-position/StartPositionPicker.svelte?svelte&type=style&lang.css";

