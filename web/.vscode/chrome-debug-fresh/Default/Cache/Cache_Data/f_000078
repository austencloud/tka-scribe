import { createHotContext as __vite__createHotContext } from "/@vite/client";import.meta.hot = __vite__createHotContext("/src/lib/components/about/ResourcesHistorian.svelte");import "/node_modules/.vite/deps/svelte_internal_disclose-version.js?v=608ae08a";

ResourcesHistorian[$.FILENAME] = 'src/lib/components/about/ResourcesHistorian.svelte';

import * as $ from "/node_modules/.vite/deps/svelte_internal_client.js?v=608ae08a";
import { createModalState } from "/src/lib/state/modal-state.svelte.ts";
import ResourceModal from "/src/lib/components/resource-guide/ResourceModal.svelte";
import VTGContent from "/src/lib/components/resource-guide/content/VTGContent.svelte";
import ResourceFilters from "/src/lib/components/about/resources/ResourceFilters.svelte";
import ResourceGrid from "/src/lib/components/about/resources/ResourceGrid.svelte";

import {
	resources,
	categories,
	getCategoryDisplayName,
	getLevelDisplayName
} from "/src/lib/components/about/resources/resourcesData.ts";

var root_4 = $.add_locations($.from_html(`<div class="placeholder-content s-SK_3dF6xvjBP"><p class="s-SK_3dF6xvjBP">9 Square Theory content coming soon...</p></div>`), ResourcesHistorian[$.FILENAME], [[177, 4, [[178, 6]]]]);

var root = $.add_locations(
	$.from_html(
		`<section class="resources-links s-SK_3dF6xvjBP"><div class="container s-SK_3dF6xvjBP"><h2 class="s-SK_3dF6xvjBP">Flow Arts Historian</h2> <p class="subtitle s-SK_3dF6xvjBP">Your comprehensive guide to flow arts resources, from cutting-edge
      learning platforms and active communities to essential vendors and
      historical archives preserving our art form's rich heritage.</p> <!> <!></div></section> <!>`,
		1
	),
	ResourcesHistorian[$.FILENAME],
	[[153, 0, [[154, 2, [[155, 4], [156, 4]]]]]]
);

function ResourcesHistorian($$anchor, $$props) {
	$.check_target(new.target);
	$.push($$props, true, ResourcesHistorian);

	let searchTerm = $.tag($.state(""), 'searchTerm');
	let selectedCategory = $.tag($.state("all"), 'selectedCategory');
	let selectedLevel = $.tag($.state("all"), 'selectedLevel');
	const modalState = createModalState();

	let filteredResources = $.tag(
		$.derived(() => {
			return resources.filter((resource) => {
				const matchesSearch = $.strict_equals($.get(searchTerm), "") || resource.name.toLowerCase().includes($.get(searchTerm).toLowerCase()) || resource.description.toLowerCase().includes($.get(searchTerm).toLowerCase()) || resource.specialties && resource.specialties.some((s) => s.toLowerCase().includes($.get(searchTerm).toLowerCase()));
				const matchesCategory = $.strict_equals($.get(selectedCategory), "all") || $.strict_equals(resource.category, $.get(selectedCategory));
				const matchesLevel = $.strict_equals($.get(selectedLevel), "all") || $.strict_equals(resource.level, $.get(selectedLevel));

				return matchesSearch && matchesCategory && matchesLevel;
			});
		}),
		'filteredResources'
	);

	function handleOpenModal(resource) {
		if (resource.hasLandingPage && resource.landingPageUrl) {
			const resourceName = resource.landingPageUrl.split("/").pop();

			if (resourceName) {
				openResourceModal(resourceName, resource);
			}
		}
	}

	function openResourceModal(resourceName, resource) {
		modalState.openModal(resourceName);

		const modalData = getModalDataForResource(resourceName, resource);

		modalState.setModalData(modalData);
	}

	function getModalDataForResource(resourceName, resource) {
		return {
			title: resource.name,
			subtitle: resource.description,
			creator: getCreatorForResource(resourceName, resource),
			category: getCategoryDisplayName(resource.category),
			level: getLevelDisplayName(resource.level),
			description: resource.value,
			keywords: getKeywordsForResource(resourceName),
			url: resource.url,
			resourceName,
			tableOfContents: getTableOfContentsForResource(resourceName),
			relatedResources: getRelatedResourcesForResource(resourceName),
			heroGradient: getHeroGradientForResource(resourceName, resource),
			creatorColor: getCreatorColorForResource(resourceName, resource)
		};
	}

	function getCreatorForResource(resourceName, resource) {
		if ($.strict_equals(resource.status, "vendor")) {
			return `Founded ${resource.foundingYear} \u2022 ${resource.companyLocation}`;
		}

		if ($.strict_equals(resource.status, "historical")) {
			return `Last Updated: ${resource.lastUpdated}`;
		}

		switch (resourceName) {
			case "vulcan-tech-gospel":
				return "Noel Yee";

			case "charlie-cushing-9-square-theory":
				return "Charlie Cushing";

			default:
				return "Community Resource";
		}
	}

	function getKeywordsForResource(resourceName) {
		switch (resourceName) {
			case "vulcan-tech-gospel":
				return "Vulcan Tech Gospel, VTG, poi theory, Noel Yee, poi flowers, transition theory, technical poi, flow arts theory";

			case "charlie-cushing-9-square-theory":
				return "9 square theory, Charlie Cushing, poi theory, unit circles, technical poi, helicopter pilot, LanternSmith, advanced poi, spatial relationships";

			default:
				return "flow arts, poi, theory, tutorial";
		}
	}

	function getTableOfContentsForResource(resourceName) {
		switch (resourceName) {
			case "vulcan-tech-gospel":
				return [
					{ id: "overview", label: "Overview" },
					{ id: "key-concepts", label: "Key Concepts" },
					{ id: "getting-started", label: "Getting Started" },
					{ id: "advanced-applications", label: "Advanced Applications" },
					{ id: "community-impact", label: "Community Impact" },
					{ id: "official-resources", label: "Official Resources" }
				];

			case "charlie-cushing-9-square-theory":
				return [
					{ id: "overview", label: "Overview" },
					{ id: "creator-background", label: "Creator Background" },
					{ id: "key-concepts", label: "Key Concepts" },
					{ id: "getting-started", label: "Getting Started" },
					{ id: "advanced-applications", label: "Advanced Applications" },
					{ id: "official-resources", label: "Official Resources" }
				];

			default:
				return [];
		}
	}

	function getRelatedResourcesForResource(resourceName) {
		switch (resourceName) {
			case "vulcan-tech-gospel":
				return [
					{
						name: "Charlie Cushing's 9 Square Theory",
						url: "/about#charlie-cushing-9-square-theory",
						description: "Advanced framework building upon VTG principles for connecting unit circles",
						type: "internal"
					},

					{
						name: "Noel Yee's Official VTG Resources",
						url: "https://noelyee.com/instruction/vulcan-tech-gospel/",
						description: "Original VTG documentation and tutorials by the creator",
						type: "external"
					}
				];

			default:
				return [];
		}
	}

	function getHeroGradientForResource(resourceName, resource) {
		switch (resource.category) {
			case "vendors":
				return "linear-gradient(135deg, rgba(76, 175, 80, 0.05) 0%, rgba(139, 195, 74, 0.05) 100%)";

			case "historical-archives":
				return "linear-gradient(135deg, rgba(158, 158, 158, 0.05) 0%, rgba(117, 117, 117, 0.05) 100%)";

			default:
				return "linear-gradient(135deg, rgba(168, 28, 237, 0.05) 0%, rgba(74, 144, 226, 0.05) 100%)";
		}
	}

	function getCreatorColorForResource(resourceName, resource) {
		switch (resource.category) {
			case "vendors":
				return "#4caf50";

			case "historical-archives":
				return "#757575";

			default:
				return "var(--primary-color)";
		}
	}

	var fragment = root();
	var section = $.first_child(fragment);
	var div = $.child(section);
	var node = $.sibling($.child(div), 4);

	$.add_svelte_meta(
		() => ResourceFilters(node, {
			get searchTerm() {
				return $.get(searchTerm);
			},

			set searchTerm($$value) {
				$.set(searchTerm, $$value, true);
			},

			get selectedCategory() {
				return $.get(selectedCategory);
			},

			set selectedCategory($$value) {
				$.set(selectedCategory, $$value, true);
			},

			get selectedLevel() {
				return $.get(selectedLevel);
			},

			set selectedLevel($$value) {
				$.set(selectedLevel, $$value, true);
			}
		}),
		'component',
		ResourcesHistorian,
		162,
		4,
		{ componentTag: 'ResourceFilters' }
	);

	var node_1 = $.sibling(node, 2);

	$.add_svelte_meta(
		() => ResourceGrid(node_1, {
			get resources() {
				return $.get(filteredResources);
			},

			onOpenModal: handleOpenModal
		}),
		'component',
		ResourcesHistorian,
		164,
		4,
		{ componentTag: 'ResourceGrid' }
	);

	$.reset(div);
	$.reset(section);

	var node_2 = $.sibling(section, 2);

	$.add_svelte_meta(
		() => ResourceModal(node_2, {
			get isOpen() {
				return modalState.isOpen;
			},

			get modalData() {
				return modalState.modalData;
			},

			onClose: () => modalState.closeModal(),

			children: $.wrap_snippet(ResourcesHistorian, ($$anchor, $$slotProps) => {
				var fragment_1 = $.comment();
				var node_3 = $.first_child(fragment_1);

				{
					var consequent = ($$anchor) => {
						var fragment_2 = $.comment();
						var node_4 = $.first_child(fragment_2);

						$.add_svelte_meta(() => VTGContent(node_4, {}), 'component', ResourcesHistorian, 175, 4, { componentTag: 'VTGContent' });
						$.append($$anchor, fragment_2);
					};

					var alternate = ($$anchor) => {
						var fragment_3 = $.comment();
						var node_5 = $.first_child(fragment_3);

						{
							var consequent_1 = ($$anchor) => {
								var div_1 = root_4();

								$.append($$anchor, div_1);
							};

							$.add_svelte_meta(
								() => $.if(
									node_5,
									($$render) => {
										if ($.strict_equals(modalState.resourceName, "charlie-cushing-9-square-theory")) $$render(consequent_1);
									},
									true
								),
								'if',
								ResourcesHistorian,
								176,
								2
							);
						}

						$.append($$anchor, fragment_3);
					};

					$.add_svelte_meta(
						() => $.if(node_3, ($$render) => {
							if ($.strict_equals(modalState.resourceName, "vulcan-tech-gospel")) $$render(consequent); else $$render(alternate, false);
						}),
						'if',
						ResourcesHistorian,
						174,
						2
					);
				}

				$.append($$anchor, fragment_1);
			}),

			$$slots: { default: true }
		}),
		'component',
		ResourcesHistorian,
		169,
		0,
		{ componentTag: 'ResourceModal' }
	);

	$.append($$anchor, fragment);

	return $.pop({ ...$.legacy_api() });
}

if (import.meta.hot) {
	ResourcesHistorian = $.hmr(ResourcesHistorian, () => ResourcesHistorian[$.HMR].source);

	import.meta.hot.acceptExports(["default"],(module) => {
		$.cleanup_styles('s-SK_3dF6xvjBP');
		module.default[$.HMR].source = ResourcesHistorian[$.HMR].source;
		$.set(ResourcesHistorian[$.HMR].source, module.default[$.HMR].original);
	});
}

export default ResourcesHistorian;
import "/src/lib/components/about/ResourcesHistorian.svelte?svelte&type=style&lang.css";

//# sourceMappingURL=data:application/json;base64,