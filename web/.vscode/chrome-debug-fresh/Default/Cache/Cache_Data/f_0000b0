import { createHotContext as __vite__createHotContext } from "/@vite/client";import.meta.hot = __vite__createHotContext("/src/lib/components/pictograph/ArrowSvg.svelte");import "/node_modules/.vite/deps/svelte_internal_disclose-version.js?v=608ae08a";

ArrowSvg[$.FILENAME] = 'src/lib/components/pictograph/ArrowSvg.svelte';

import * as $ from "/node_modules/.vite/deps/svelte_internal_client.js?v=608ae08a";
import { MotionColor } from "/src/lib/domain/index.ts";
import { resolve } from "/src/lib/services/bootstrap.ts";
import { onMount } from "/node_modules/.vite/deps/svelte.js?v=608ae08a";

var root_1 = $.add_locations($.from_svg(`<circle r="10" fill="red" opacity="0.5" class="s-DGO6GOA5RSra"></circle><text x="0" y="4" text-anchor="middle" font-size="8" fill="white" class="s-DGO6GOA5RSra">!</text>`, 1), ArrowSvg[$.FILENAME], [[85, 4], [86, 4]]);
var root_3 = $.add_locations($.from_svg(`<text x="0" y="4" text-anchor="middle" font-size="10" fill="gray" opacity="0.5" class="s-DGO6GOA5RSra">No motion data</text>`), ArrowSvg[$.FILENAME], [[89, 4]]);
var root_5 = $.add_locations($.from_svg(`<circle r="8" opacity="0.3" class="s-DGO6GOA5RSra"></circle><animate attributeName="opacity" values="0.3;0.8;0.3" dur="1s" repeatCount="indefinite" class="s-DGO6GOA5RSra"></animate>`, 1), ArrowSvg[$.FILENAME], [[101, 4], [106, 4]]);
var root_7 = $.add_locations($.from_svg(`<g><!></g>`), ArrowSvg[$.FILENAME], [[115, 4]]);
var root_8 = $.add_locations($.from_svg(`<g opacity="0" aria-hidden="true" class="s-DGO6GOA5RSra"><circle r="2" opacity="0.1" class="s-DGO6GOA5RSra"></circle></g>`), ArrowSvg[$.FILENAME], [[133, 4, [[134, 6]]]]);
var root = $.add_locations($.from_svg(`<g><!></g>`), ArrowSvg[$.FILENAME], [[76, 0]]);

function ArrowSvg($$anchor, $$props) {
	$.check_target(new.target);
	$.push($$props, true, ArrowSvg);

	let showArrow = $.prop($$props, 'showArrow', 3, true);
	const arrowRenderingService = resolve("IArrowRenderingService");
	let loaded = $.tag($.state(false), 'loaded');
	let error = $.tag($.state(null), 'error');
	let svgData = $.tag($.state(null), 'svgData');

	const position = $.tag(
		$.derived(() => () => {
			if (!$$props.motionData) return { x: 475, y: 475, rotation: 0 };

			if ($$props.preCalculatedPosition) {
				return $$props.preCalculatedPosition;
			}

			const arrowPlacement = $$props.motionData.arrowPlacementData;

			if ($.strict_equals(arrowPlacement.positionX, 0, false) || $.strict_equals(arrowPlacement.positionY, 0, false)) {
				return {
					x: arrowPlacement.positionX,
					y: arrowPlacement.positionY,
					rotation: arrowPlacement.rotationAngle || 0
				};
			}

			return { x: 475, y: 475, rotation: 0 };
		}),
		'position'
	);

	const calculatedPosition = $.tag($.derived(() => () => $.get(position)()), 'calculatedPosition');
	const shouldMirror = $.tag($.derived(() => () => $$props.preCalculatedMirroring ?? false), 'shouldMirror');

	const loadSvg = async () => {
		try {
			if (!$$props.motionData || !$$props.motionData.arrowPlacementData) throw new Error("No motion data or arrow placement data available");

			console.log(...$.log_if_contains_state('log', "\u{1F3F9} ArrowSvg: Loading arrow data for motion:", $$props.motionData
				? {
					motionType: $$props.motionData.motionType,
					color: $$props.motionData.color,
					turns: $$props.motionData.turns
				}
				: "undefined"));

			const svgDataResult = (await $.track_reactivity_loss(arrowRenderingService.loadArrowPlacementData($$props.motionData.arrowPlacementData, $$props.motionData)))();

			console.log(...$.log_if_contains_state('log', "\u2705 ArrowSvg: Arrow SVG data loaded successfully:", svgDataResult));
			$.set(svgData, svgDataResult, true);
			$.set(loaded, true);
			$$props.onLoaded?.(`${$$props.motionData?.color}-arrow`);
		} catch(e) {
			console.error(...$.log_if_contains_state('error', "\u274C ArrowSvg: Error loading arrow data:", e));
			$.set(error, `Failed to load arrow SVG: ${e}`);
			$$props.onError?.(`${$$props.motionData?.color}-arrow`, $.get(error));

			throw e;
		}
	};

	onMount(() => {
		loadSvg();
	});

	var g = root();
	let classes;
	var node = $.child(g);

	{
		var consequent = ($$anchor) => {
			var fragment = root_1();

			$.next();
			$.append($$anchor, fragment);
		};

		var alternate_3 = ($$anchor) => {
			var fragment_1 = $.comment();
			var node_1 = $.first_child(fragment_1);

			{
				var consequent_1 = ($$anchor) => {
					var text = root_3();

					$.append($$anchor, text);
				};

				var alternate_2 = ($$anchor) => {
					var fragment_2 = $.comment();
					var node_2 = $.first_child(fragment_2);

					{
						var consequent_2 = ($$anchor) => {
							var fragment_3 = root_5();
							var circle = $.first_child(fragment_3);

							$.next();
							$.template_effect(() => $.set_attribute(circle, 'fill', $.strict_equals($$props.motionData?.color, MotionColor.BLUE) ? "#2E3192" : "#ED1C24"));
							$.append($$anchor, fragment_3);
						};

						var alternate_1 = ($$anchor) => {
							var fragment_4 = $.comment();
							var node_3 = $.first_child(fragment_4);

							{
								var consequent_3 = ($$anchor) => {
									var g_1 = root_7();
									let classes_1;
									let styles;
									var node_4 = $.child(g_1);

									$.html(node_4, () => $.get(svgData).imageSrc, true);
									$.reset(g_1);

									$.template_effect(
										($0, $1, $2, $3, $4, $5) => {
											$.set_attribute(g_1, 'transform', `
        translate(${$0 ?? ''}, ${$1 ?? ''})
        rotate(${$2 ?? ''})
        scale(${$3 ?? ''}, 1)
        translate(${-$.get(svgData).center.x}, ${-$.get(svgData).center.y})
      `);

											classes_1 = $.set_class(g_1, 0, `arrow-svg ${$$props.motionData?.color ?? ''}-arrow-svg`, 's-DGO6GOA5RSra', classes_1, $4);
											styles = $.set_style(g_1, '', styles, $5);
										},
										[
											() => $.get(calculatedPosition)().x,
											() => $.get(calculatedPosition)().y,
											() => $.get(calculatedPosition)().rotation || $$props.motionData?.arrowPlacementData?.rotationAngle || 0,
											() => $.get(shouldMirror)() ? -1 : 1,
											() => ({ mirrored: $.get(shouldMirror) }),
											() => ({ opacity: showArrow() ? 1 : 0 })
										]
									);

									$.append($$anchor, g_1);
								};

								var alternate = ($$anchor) => {
									var g_2 = root_8();
									var circle_1 = $.child(g_2);

									$.reset(g_2);
									$.template_effect(() => $.set_attribute(circle_1, 'fill', $.strict_equals($$props.motionData?.color, MotionColor.BLUE) ? "#2E3192" : "#ED1C24"));
									$.append($$anchor, g_2);
								};

								$.add_svelte_meta(
									() => $.if(
										node_3,
										($$render) => {
											if (showArrow()) $$render(consequent_3); else $$render(alternate, false);
										},
										true
									),
									'if',
									ArrowSvg,
									112,
									2
								);
							}

							$.append($$anchor, fragment_4);
						};

						$.add_svelte_meta(
							() => $.if(
								node_2,
								($$render) => {
									if (!$.get(loaded) || !$.get(svgData)) $$render(consequent_2); else $$render(alternate_1, false);
								},
								true
							),
							'if',
							ArrowSvg,
							99,
							2
						);
					}

					$.append($$anchor, fragment_2);
				};

				$.add_svelte_meta(
					() => $.if(
						node_1,
						($$render) => {
							if (!$$props.motionData) $$render(consequent_1); else $$render(alternate_2, false);
						},
						true
					),
					'if',
					ArrowSvg,
					87,
					2
				);
			}

			$.append($$anchor, fragment_1);
		};

		$.add_svelte_meta(
			() => $.if(node, ($$render) => {
				if ($.get(error)) $$render(consequent); else $$render(alternate_3, false);
			}),
			'if',
			ArrowSvg,
			83,
			2
		);
	}

	$.reset(g);

	$.template_effect(
		($0) => {
			classes = $.set_class(g, 0, `arrow-group ${$$props.motionData?.color ?? ''}-arrow`, 's-DGO6GOA5RSra', classes, $0);
			$.set_attribute(g, 'data-arrow-color', $$props.motionData?.color);
			$.set_attribute(g, 'data-motion-type', $$props.motionData?.motionType);
			$.set_attribute(g, 'data-location', $$props.motionData?.arrowLocation);
		},
		[() => ({ loaded: $.get(loaded) })]
	);

	$.append($$anchor, g);

	return $.pop({ ...$.legacy_api() });
}

if (import.meta.hot) {
	ArrowSvg = $.hmr(ArrowSvg, () => ArrowSvg[$.HMR].source);

	import.meta.hot.acceptExports(["default"],(module) => {
		$.cleanup_styles('s-DGO6GOA5RSra');
		module.default[$.HMR].source = ArrowSvg[$.HMR].source;
		$.set(ArrowSvg[$.HMR].source, module.default[$.HMR].original);
	});
}

