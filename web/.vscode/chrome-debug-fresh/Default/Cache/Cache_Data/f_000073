							if ($$props.error) $$render(consequent_1); else $$render(alternate_1, false);
						},
						true
					),
					'if',
					PageDisplay,
					77,
					2
				);
			}

			$.append($$anchor, fragment);
		};

		$.add_svelte_meta(
			() => $.if(node, ($$render) => {
				if ($$props.isLoading) $$render(consequent); else $$render(alternate_2, false);
			}),
			'if',
			PageDisplay,
			70,
			2
		);
	}

	$.reset(div);
	$.bind_this(div, ($$value) => containerElement = $$value, () => containerElement);
	$.append($$anchor, div);

	return $.pop({ ...$.legacy_api() });
}

