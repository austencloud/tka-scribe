function PageDisplay($$anchor, $$props) {
	$.check_target(new.target);
	$.push($$props, true, PageDisplay);

	let columnCount = $.prop($$props, 'columnCount', 3, 1);

	// Default to 1 page per row
	let containerElement;

	let pageWidth = $.tag($.state(816), 'pageWidth');
	let pageHeight = $.tag($.state(1056), 'pageHeight');
	let resizeObserver = null;

	function updatePageSize() {
		if (containerElement && columnCount() > 0) {
			const containerWidth = containerElement.clientWidth;
			const containerHeight = containerElement.clientHeight;
			const totalPadding = 40;
			const totalGaps = (columnCount() - 1) * 20;
			const availableWidth = containerWidth - totalPadding - totalGaps;
			const calculatedPageWidth = Math.max(300, availableWidth / columnCount());
			const pagePadding = 48 + 24;
			const estimatedSequenceHeight = calculatedPageWidth * 0.4;
			const gridGap = 10;
			const rowsNeeded = 3;
			const contentHeight = estimatedSequenceHeight * rowsNeeded + gridGap * (rowsNeeded - 1);
			const calculatedPageHeight = contentHeight + pagePadding;
			const maxHeight = containerHeight - 40;

			if (calculatedPageHeight > maxHeight) {
				const scaleFactor = maxHeight / calculatedPageHeight;

				$.set(pageHeight, maxHeight);
				$.set(pageWidth, calculatedPageWidth * scaleFactor);
			} else {
				$.set(pageWidth, calculatedPageWidth, true);
				$.set(pageHeight, calculatedPageHeight);
			}
		}
	}

	$.user_effect(() => {
		if (containerElement) {
			updatePageSize();

			resizeObserver = new ResizeObserver(() => {
				updatePageSize();
			});

			resizeObserver.observe(containerElement);

			return () => {
				if (resizeObserver) {
					resizeObserver.disconnect();
					resizeObserver = null;
				}
			};
		}
	});

	$.user_effect(() => {
		updatePageSize();
	});

	$.user_effect(() => {
		const handleWindowResize = () => {
			setTimeout(updatePageSize, 10);
		};

		window.addEventListener("resize", handleWindowResize);

		return () => {
			window.removeEventListener("resize", handleWindowResize);
		};
	});

	var div = root();
	var node = $.child(div);

	{
		var consequent = ($$anchor) => {
			var div_1 = root_1();

			$.append($$anchor, div_1);
		};

		var alternate_2 = ($$anchor) => {
			var fragment = $.comment();
			var node_1 = $.first_child(fragment);

			{
				var consequent_1 = ($$anchor) => {
					var div_2 = root_3();
					var p = $.sibling($.child(div_2), 4);
					var text = $.child(p, true);

					$.reset(p);

					var button = $.sibling(p, 2);

					button.__click = function (...$$args) {
						$.apply(() => $$props.onRetry, this, $$args, PageDisplay, [83, 44]);
					};

					$.reset(div_2);
					$.template_effect(() => $.set_text(text, $$props.error));
					$.append($$anchor, div_2);
				};

				var alternate_1 = ($$anchor) => {
					var fragment_1 = $.comment();
					var node_2 = $.first_child(fragment_1);

					{
						var consequent_2 = ($$anchor) => {
							var div_3 = root_5();

							$.append($$anchor, div_3);
						};

						var alternate = ($$anchor) => {
							var div_4 = root_6();
							let styles;

							$.validate_each_keys(() => $$props.pages, (page) => page.id);

							$.add_svelte_meta(
								() => $.each(div_4, 21, () => $$props.pages, (page) => page.id, ($$anchor, page) => {
									var div_5 = root_7();
									let styles_1;
									var div_6 = $.child(div_5);
									var h3 = $.child(div_6);
									var text_1 = $.child(h3);

									$.reset(h3);

									var span = $.sibling(h3, 2);
									var text_2 = $.child(span);

									$.reset(span);
									$.reset(div_6);

									var div_7 = $.sibling(div_6, 2);
									var div_8 = $.child(div_7);

									$.set_style(div_8, '', {}, { 'grid-template-columns': 'repeat(2, 1fr)' });
									$.validate_each_keys(() => $.get(page).sequences, (sequence) => sequence.id);

									$.add_svelte_meta(
										() => $.each(div_8, 21, () => $.get(page).sequences, (sequence) => sequence.id, ($$anchor, sequence) => {
											var fragment_2 = $.comment();
											var node_3 = $.first_child(fragment_2);

											$.add_svelte_meta(
												() => SequenceCard(node_3, {
													get sequence() {
														return $.get(sequence);
													}
												}),
												'component',
												PageDisplay,
												122,
												16,
												{ componentTag: 'SequenceCard' }
											);

											$.append($$anchor, fragment_2);
										}),
										'each',
										PageDisplay,
										121,
										14
									);

									$.reset(div_8);
									$.reset(div_7);
									$.reset(div_5);

									$.template_effect(
										($0, $1) => {
											$.set_attribute(div_5, 'data-page-id', $.get(page).id);
											styles_1 = $.set_style(div_5, '', styles_1, $0);
											$.set_text(text_1, `Page ${$1 ?? ''}`);
											$.set_text(text_2, `${$.get(page).sequences.length ?? ''} sequences ‚Ä¢ 2 columns`);
										},
										[
											() => ({
												width: `${$.get(pageWidth) ?? ''}px`,
												height: `${$.get(pageHeight) ?? ''}px`
											}),

											() => $.get(page).id.replace("page-", "")
										]
									);

									$.append($$anchor, div_5);
								}),
								'each',
								PageDisplay,
								100,
								6
							);

							$.reset(div_4);

							$.template_effect(($0) => styles = $.set_style(div_4, '', styles, $0), [
								() => ({ 'grid-template-columns': `repeat(${columnCount()}, 1fr)` })
							]);

							$.append($$anchor, div_4);
						};

						$.add_svelte_meta(
							() => $.if(
								node_2,
								($$render) => {
									if ($.strict_equals($$props.pages.length, 1) && $$props.pages[0].isEmpty) $$render(consequent_2); else $$render(alternate, false);
								},
