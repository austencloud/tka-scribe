var root = $.add_locations($.from_html(`<div class="export-preview-card s-L7RYEnxRE7Dp"><div class="preview-header s-L7RYEnxRE7Dp"><h3 class="card-title s-L7RYEnxRE7Dp">Export Preview</h3> <button class="refresh-button s-L7RYEnxRE7Dp" title="Refresh preview"><span>üîÑ</span></button></div> <div class="preview-content s-L7RYEnxRE7Dp"><!></div> <div class="preview-footer s-L7RYEnxRE7Dp"><div class="status-bar s-L7RYEnxRE7Dp"><span class="status-text s-L7RYEnxRE7Dp"> </span></div> <!></div></div>`), ExportPreviewCard[$.FILENAME], [
