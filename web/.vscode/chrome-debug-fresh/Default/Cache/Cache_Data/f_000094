ExportPreviewCard[$.FILENAME] = 'src/lib/components/export/ExportPreviewCard.svelte';

import * as $ from "/node_modules/.vite/deps/svelte_internal_client.js?v=608ae08a";

function handleRefreshPreview() {
	console.log("\u{1F504} [PREVIEW-CARD] Manual refresh requested");
}

var root_1 = $.add_locations($.from_html(`<div class="preview-loading s-L7RYEnxRE7Dp"><div class="loading-spinner s-L7RYEnxRE7Dp"></div> <p class="s-L7RYEnxRE7Dp">Generating preview...</p> <span class="loading-hint s-L7RYEnxRE7Dp">Creating image from sequence...</span></div>`), ExportPreviewCard[$.FILENAME], [[86, 6, [[87, 8], [88, 8], [89, 8]]]]);
var root_3 = $.add_locations($.from_html(`<div class="preview-error s-L7RYEnxRE7Dp"><div class="error-icon s-L7RYEnxRE7Dp">‚ùå</div> <p class="s-L7RYEnxRE7Dp">Preview generation failed</p> <span class="error-details s-L7RYEnxRE7Dp"> </span></div>`), ExportPreviewCard[$.FILENAME], [[92, 6, [[93, 8], [94, 8], [95, 8]]]]);
var root_6 = $.add_locations($.from_html(`<span class="validation-error s-L7RYEnxRE7Dp"> </span>`), ExportPreviewCard[$.FILENAME], [[103, 12]]);
