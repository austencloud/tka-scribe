]);

function ExportPreviewCard($$anchor, $$props) {
	$.check_target(new.target);
	$.push($$props, true, ExportPreviewCard);

	let previewImageUrl = $.prop($$props, 'previewImageUrl', 3, null),
		isGeneratingPreview = $.prop($$props, 'isGeneratingPreview', 3, false),
		previewError = $.prop($$props, 'previewError', 3, null),
		validationErrors = $.prop($$props, 'validationErrors', 19, () => []);

	let previewInfo = $.tag(
		$.derived(() => () => {
			if (!$$props.currentSequence) return null;

			const enabledOptions = Object.entries($$props.exportSettings).filter(([_key, value]) => $.strict_equals(typeof value, "boolean") && value).length;

			return {
				sequenceName: $$props.currentSequence.name || $$props.currentSequence.word || "Untitled Sequence",
				beatCount: $$props.currentSequence.beats?.length || 0,
				format: $$props.exportSettings.export_format,
				quality: $$props.exportSettings.export_quality,
				enabledOptions,
				hasStartPosition: !!$$props.currentSequence.startPosition,
				isEmpty: $.strict_equals($$props.currentSequence.beats?.length || 0, 0)
			};
		}),
		'previewInfo'
	);

	let statusMessage = $.tag(
		$.derived(() => () => {
			if (isGeneratingPreview()) return "Generating preview...";
			if (previewError()) return `Error: ${previewError()}`;

			if (validationErrors() && validationErrors().length > 0) {
				return `Validation errors: ${validationErrors().join(", ")}`;
			}

			if (!$$props.currentSequence) {
				return "Create a sequence to see preview";
			}

			const info = $.get(previewInfo)();

			if (info) {
				if (info.isEmpty && !$$props.exportSettings.include_start_position) {
					return "Empty sequence - enable start position to preview";
				}

				if (info.isEmpty && $$props.exportSettings.include_start_position) {
					return "Start position only preview";
				}

				return `Preview: ${info.format} \u2022 ${info.quality} \u2022 ${info.enabledOptions} options enabled`;
			}

			return "Preview ready";
		}),
		'statusMessage'
	);

	let sequenceDetails = $.tag(
		$.derived(() => () => {
			const info = $.get(previewInfo)();

			if (!info) return null;

			return [
				{ label: "Sequence", value: info.sequenceName },
				{ label: "Beats", value: info.beatCount.toString() },
				{ label: "Format", value: info.format },
				{ label: "Quality", value: info.quality },

				{
					label: "Start Position",
					value: info.hasStartPosition ? "Yes" : "No"
				}
			];
		}),
		'sequenceDetails'
	);

	let previewDisplayState = $.tag(
		$.derived(() => () => {
			if (isGeneratingPreview()) return "loading";
			if (previewError()) return "error";
			if (validationErrors() && validationErrors().length > 0) return "validation-error";
			if (previewImageUrl()) return "image";
			if (!$$props.currentSequence) return "no-sequence";

			return "placeholder";
		}),
		'previewDisplayState'
	);

	var div = root();
	var div_1 = $.child(div);
	var button = $.sibling($.child(div_1), 2);

	button.__click = [handleRefreshPreview];

	var span = $.child(button);
	let classes;

	$.reset(button);
	$.reset(div_1);

	var div_2 = $.sibling(div_1, 2);
	var node = $.child(div_2);

	{
		var consequent = ($$anchor) => {
			var div_3 = root_1();

			$.append($$anchor, div_3);
		};

		var alternate_4 = ($$anchor) => {
			var fragment = $.comment();
			var node_1 = $.first_child(fragment);

			{
				var consequent_1 = ($$anchor) => {
					var div_4 = root_3();
					var span_1 = $.sibling($.child(div_4), 4);
					var text = $.child(span_1, true);

					$.reset(span_1);
					$.reset(div_4);
					$.template_effect(() => $.set_text(text, previewError()));
					$.append($$anchor, div_4);
				};

				var alternate_3 = ($$anchor) => {
					var fragment_1 = $.comment();
					var node_2 = $.first_child(fragment_1);

					{
						var consequent_2 = ($$anchor) => {
							var div_5 = root_5();
							var div_6 = $.sibling($.child(div_5), 4);

							$.add_svelte_meta(
								() => $.each(div_6, 21, validationErrors, $.index, ($$anchor, error) => {
									var span_2 = root_6();
									var text_1 = $.child(span_2, true);

									$.reset(span_2);
									$.template_effect(() => $.set_text(text_1, $.get(error)));
									$.append($$anchor, span_2);
								}),
								'each',
								ExportPreviewCard,
								102,
								10
							);

							$.reset(div_6);
							$.reset(div_5);
							$.append($$anchor, div_5);
						};

						var alternate_2 = ($$anchor) => {
							var fragment_2 = $.comment();
							var node_3 = $.first_child(fragment_2);

							{
								var consequent_4 = ($$anchor) => {
									var div_7 = root_8();
									var img = $.child(div_7);
									var div_8 = $.sibling(img, 2);
									var div_9 = $.child(div_8);
									var node_4 = $.child(div_9);

									{
										var consequent_3 = ($$anchor) => {
											const info = $.tag($.derived(() => $.get(previewInfo)()), 'info');

											$.get(info);

											var fragment_3 = root_9();
											var span_3 = $.first_child(fragment_3);
											var text_2 = $.child(span_3, true);

											$.reset(span_3);

											var span_4 = $.sibling(span_3, 2);
											var text_3 = $.child(span_4);

											$.reset(span_4);

											$.template_effect(() => {
												$.set_text(text_2, $.get(info)?.format);
												$.set_text(text_3, `${$.get(info)?.beatCount ?? ''} beats`);
											});

											$.append($$anchor, fragment_3);
										};

										$.add_svelte_meta(
											() => $.if(node_4, ($$render) => {
												if ($.get(previewInfo)()) $$render(consequent_3);
											}),
											'if',
											ExportPreviewCard,
											112,
											12
										);
									}

									$.reset(div_9);
									$.reset(div_8);
									$.reset(div_7);
									$.template_effect(() => $.set_attribute(img, 'src', previewImageUrl()));
									$.append($$anchor, div_7);
								};

								var alternate_1 = ($$anchor) => {
									var fragment_4 = $.comment();
									var node_5 = $.first_child(fragment_4);

									{
										var consequent_5 = ($$anchor) => {
											var div_10 = root_11();

											$.append($$anchor, div_10);
										};

										var alternate = ($$anchor) => {
											var div_11 = root_12();

											$.append($$anchor, div_11);
										};

										$.add_svelte_meta(
											() => $.if(
												node_5,
												($$render) => {
													if ($.strict_equals($.get(previewDisplayState)(), "no-sequence")) $$render(consequent_5); else $$render(alternate, false);
												},
												true
											),
											'if',
											ExportPreviewCard,
											120,
											4
										);
									}

									$.append($$anchor, fragment_4);
								};

								$.add_svelte_meta(
									() => $.if(
										node_3,
										($$render) => {
											if ($.strict_equals($.get(previewDisplayState)(), "image")) $$render(consequent_4); else $$render(alternate_1, false);
										},
										true
									),
									'if',
									ExportPreviewCard,
									107,
									4
								);
							}

							$.append($$anchor, fragment_2);
						};

						$.add_svelte_meta(
							() => $.if(
								node_2,
								($$render) => {
									if ($.strict_equals($.get(previewDisplayState)(), "validation-error")) $$render(consequent_2); else $$render(alternate_2, false);
								},
								true
							),
							'if',
							ExportPreviewCard,
							97,
							4
						);
					}

					$.append($$anchor, fragment_1);
				};

				$.add_svelte_meta(
					() => $.if(
						node_1,
						($$render) => {
							if ($.strict_equals($.get(previewDisplayState)(), "error")) $$render(consequent_1); else $$render(alternate_3, false);
						},
						true
					),
					'if',
					ExportPreviewCard,
					91,
					4
				);
			}

			$.append($$anchor, fragment);
		};

		$.add_svelte_meta(
			() => $.if(node, ($$render) => {
				if ($.strict_equals($.get(previewDisplayState)(), "loading")) $$render(consequent); else $$render(alternate_4, false);
			}),
			'if',
			ExportPreviewCard,
			85,
			4
		);
	}

	$.reset(div_2);

	var div_12 = $.sibling(div_2, 2);
	var div_13 = $.child(div_12);
	var span_5 = $.child(div_13);
	var text_4 = $.child(span_5, true);

	$.reset(span_5);
	$.reset(div_13);

	var node_6 = $.sibling(div_13, 2);

	{
		var consequent_7 = ($$anchor) => {
			const details = $.tag($.derived(() => $.get(sequenceDetails)()), 'details');

			$.get(details);

			var fragment_5 = $.comment();
			var node_7 = $.first_child(fragment_5);

			{
				var consequent_6 = ($$anchor) => {
					var div_14 = root_14();

					$.add_svelte_meta(
						() => $.each(div_14, 21, () => $.get(details), $.index, ($$anchor, detail) => {
							var div_15 = root_15();
							var span_6 = $.child(div_15);
							var text_5 = $.child(span_6);

							$.reset(span_6);

							var span_7 = $.sibling(span_6, 2);
							var text_6 = $.child(span_7, true);

							$.reset(span_7);
							$.reset(div_15);

							$.template_effect(() => {
								$.set_text(text_5, `${$.get(detail).label ?? ''}:`);
								$.set_text(text_6, $.get(detail).value);
							});

							$.append($$anchor, div_15);
						}),
						'each',
						ExportPreviewCard,
						148,
						10
					);

					$.reset(div_14);
					$.append($$anchor, div_14);
				};

				$.add_svelte_meta(
					() => $.if(node_7, ($$render) => {
						if ($.get(details)) $$render(consequent_6);
					}),
					'if',
					ExportPreviewCard,
					146,
					6
				);
			}

			$.append($$anchor, fragment_5);
		};

		$.add_svelte_meta(
			() => $.if(node_6, ($$render) => {
				if ($.get(sequenceDetails)()) $$render(consequent_7);
			}),
			'if',
			ExportPreviewCard,
			144,
			4
		);
	}

	$.reset(div_12);
	$.reset(div);

	$.template_effect(
		($0, $1) => {
			button.disabled = isGeneratingPreview();
			classes = $.set_class(span, 1, 'refresh-icon s-L7RYEnxRE7Dp', null, classes, $0);
			$.set_text(text_4, $1);
		},
		[
			() => ({ spinning: isGeneratingPreview() }),
			() => $.get(statusMessage)()
		]
	);

	$.append($$anchor, div);

	return $.pop({ ...$.legacy_api() });
}

if (import.meta.hot) {
	ExportPreviewCard = $.hmr(ExportPreviewCard, () => ExportPreviewCard[$.HMR].source);

	import.meta.hot.acceptExports(["default"],(module) => {
		$.cleanup_styles('s-L7RYEnxRE7Dp');
		module.default[$.HMR].source = ExportPreviewCard[$.HMR].source;
		$.set(ExportPreviewCard[$.HMR].source, module.default[$.HMR].original);
	});
}

export default ExportPreviewCard;

$.delegate(['click']);
import "/src/lib/components/export/ExportPreviewCard.svelte?svelte&type=style&lang.css";

