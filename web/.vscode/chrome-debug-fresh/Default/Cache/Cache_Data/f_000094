var root_9 = $.add_locations($.from_html(`<span class="image-size s-L7RYEnxRE7Dp"> </span> <span class="image-beats s-L7RYEnxRE7Dp"> </span>`, 1), ExportPreviewCard[$.FILENAME], [[114, 14], [115, 14]]);
var root_8 = $.add_locations($.from_html(`<div class="preview-image-container s-L7RYEnxRE7Dp"><img alt="Export preview" class="preview-image s-L7RYEnxRE7Dp"/> <div class="image-overlay s-L7RYEnxRE7Dp"><div class="image-info s-L7RYEnxRE7Dp"><!></div></div></div>`), ExportPreviewCard[$.FILENAME], [[108, 6, [[109, 8], [110, 8, [[111, 10]]]]]]);
var root_11 = $.add_locations($.from_html(`<div class="preview-placeholder s-L7RYEnxRE7Dp"><div class="placeholder-icon s-L7RYEnxRE7Dp">üìÑ</div> <p class="s-L7RYEnxRE7Dp">No sequence selected</p> <span class="placeholder-hint s-L7RYEnxRE7Dp">Create or select a sequence in the Construct tab to see preview</span></div>`), ExportPreviewCard[$.FILENAME], [[121, 6, [[122, 8], [123, 8], [124, 8]]]]);
var root_12 = $.add_locations($.from_html(`<div class="preview-placeholder s-L7RYEnxRE7Dp"><div class="placeholder-icon s-L7RYEnxRE7Dp">üìù</div> <p class="s-L7RYEnxRE7Dp">Ready to preview</p> <span class="placeholder-hint s-L7RYEnxRE7Dp">Preview will update automatically when settings change</span></div>`), ExportPreviewCard[$.FILENAME], [[129, 6, [[130, 8], [131, 8], [132, 8]]]]);
var root_15 = $.add_locations($.from_html(`<div class="detail-item s-L7RYEnxRE7Dp"><span class="detail-label s-L7RYEnxRE7Dp"> </span> <span class="detail-value s-L7RYEnxRE7Dp"> </span></div>`), ExportPreviewCard[$.FILENAME], [[149, 12, [[150, 14], [151, 14]]]]);
