      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONUNBUFFERED": "1",
        "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/src/web_app/modern_desktop;${workspaceFolder}/src/web_app;${workspaceFolder}/src;${workspaceFolder}/launcher;${workspaceFolder}/packages"
      },
      "stopOnEntry": false,
      "showReturnValue": true,
      "subProcess": true
    },
    {
      "name": "üñ•Ô∏è TKA Desktop App (Root)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONUNBUFFERED": "1",
        "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/src/web_app/modern_desktop;${workspaceFolder}/src/web_app;${workspaceFolder}/src;${workspaceFolder}/launcher;${workspaceFolder}/packages"
      },
