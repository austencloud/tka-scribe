      "request": "launch",
      "program": "${workspaceFolder}/src/web_app/modern_web/node_modules/.bin/vitest",
      "args": ["run"],
      "cwd": "${workspaceFolder}/src/web_app/modern_web",
      "env": {
        "NODE_ENV": "test"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "test"]
    },
    {
      "name": "üß™ TKA Legacy Web Tests",
      "type": "node",
