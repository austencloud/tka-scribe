{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "üöÄ TKA Modern Desktop App (F5 Default)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/desktop/modern/main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/src;${workspaceFolder}/src/desktop;${workspaceFolder}/packages;${workspaceFolder}/.venv/Lib/site-packages",
        "VIRTUAL_ENV": "${workspaceFolder}/.venv",
        "PYTHONUNBUFFERED": "1"
      },
      "stopOnEntry": false,
      "showReturnValue": true,
      "subProcess": true
    },
    {
      "name": "ÔøΩÔ∏è TKA Legacy Desktop App",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/desktop/legacy/main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/src;${workspaceFolder}/src/desktop;${workspaceFolder}/packages;${workspaceFolder}/.venv/Lib/site-packages",
        "VIRTUAL_ENV": "${workspaceFolder}/.venv",
        "PYTHONUNBUFFERED": "1"
      },
      "stopOnEntry": false,
      "showReturnValue": true,
      "subProcess": true
    },
    {
      "name": "üåê TKA Web App (Development)",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "cwd": "${workspaceFolder}/src/web",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "üß™ Run Tests",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "args": ["-v"],
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/src;${workspaceFolder}/src/desktop;${workspaceFolder}/packages;${workspaceFolder}/.venv/Lib/site-packages",
        "VIRTUAL_ENV": "${workspaceFolder}/.venv",
        "PYTHONUNBUFFERED": "1"
      },
      "stopOnEntry": false,
      "showReturnValue": true,
      "subProcess": true
    }
  ]
}
