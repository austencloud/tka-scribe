{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "üöÄ TKA Modern Desktop App (F5 Default)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/web_app/modern_desktop/main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONUNBUFFERED": "1",
        "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/src/web_app/modern_desktop;${workspaceFolder}/src/web_app;${workspaceFolder}/src;${workspaceFolder}/launcher;${workspaceFolder}/packages"
      },
      "stopOnEntry": false,
      "showReturnValue": true,
      "subProcess": true
    },
    {
      "name": "üèõÔ∏è TKA Legacy Desktop App",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/web_app/legacy_desktop/main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONUNBUFFERED": "1",
        "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/src/web_app/legacy_desktop;${workspaceFolder}/src/web_app;${workspaceFolder}/src;${workspaceFolder}/launcher;${workspaceFolder}/packages"
      },
      "stopOnEntry": false,
      "showReturnValue": true,
      "subProcess": true
    },
    {
      "name": "üåê TKA Modern Web App",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/src/web_app/modern_web/node_modules/.bin/vite",
      "args": ["dev", "--host", "localhost", "--port", "5174"],
      "cwd": "${workspaceFolder}/src/web_app/modern_web",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"]
    },
    {
      "name": "üèõÔ∏è TKA Legacy Web App",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/src/web_app/legacy_web/node_modules/.bin/vite",
      "args": ["dev", "--host", "localhost"],
      "cwd": "${workspaceFolder}/src/web_app/legacy_web",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"]
    },
    {
      "name": "üöÄ TKA Launcher",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/launcher/main.py",
      "console": "integratedTerminal",
