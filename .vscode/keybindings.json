[
  {
    "key": "ctrl+alt+t",
    "command": "workbench.action.tasks.runTask",
    "args": "Run All Tests"
  },
  {
    "key": "ctrl+alt+v",
    "command": "workbench.action.tasks.runTask",
    "args": "Validate Test Organization"
  },
  {
    "key": "ctrl+alt+f",
    "command": "workbench.action.tasks.runTask",
    "args": "Full Stack Development"
  },
  {
    "key": "ctrl+i",
    "command": "inlineChat.start",
    "when": "editorTextFocus && !inlineChatVisible"
  },
  {
    "key": "ctrl+i",
    "command": "workbench.action.terminal.chat",
    "when": "terminalFocus"
  }
]
