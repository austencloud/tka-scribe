{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "üåê TKA Modern Web App (F5 Default)",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/modern_web",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
