{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/routes/**/*.svelte",
    "src/routes/**/*.ts",
    "src/lib/features/*/index.ts",
    "src/lib/features/**/*Module.svelte",
    "src/lib/features/**/*Tab.svelte",
    "src/lib/shared/inversify/bootstrap.ts",
    "src/lib/shared/inversify/container.ts",
    "scripts/*.js",
    "scripts/*.cjs",
    "scripts/*.mjs",
    "tests/**/*.ts",
    "functions/src/index.ts",
    "vite.config.ts",
    "svelte.config.js",
    "vitest.config.ts"
  ],
  "project": [
    "src/**/*.{ts,svelte}",
    "scripts/**/*.{js,cjs,mjs}",
    "tests/**/*.ts"
  ],
  "ignore": [
    "**/*.d.ts",
    "src/stories/**",
    "docs/**",
    "deployment/**",
    "**/node_modules/**"
  ],
  "ignoreDependencies": [
    "@sveltejs/adapter-*",
    "svelte-check",
    "typescript",
    "vite",
    "vitest"
  ],
  "ignoreBinaries": [
    "firebase",
    "netlify"
  ]
}
